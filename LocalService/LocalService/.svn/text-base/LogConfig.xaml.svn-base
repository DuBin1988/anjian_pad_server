<ResourceDictionary
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation" 
    xmlns:log="clr-namespace:Com.Aote.Logs"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
    <log:ConsoleAppender x:Key="ConsoleAppender"></log:ConsoleAppender>
    <log:LogConfigs x:Key="LogConfigs">
        <log:LogConfig Appender="{StaticResource ConsoleAppender}" Name="service" Level="Debug"></log:LogConfig>
        <log:LogConfig Appender="{StaticResource ConsoleAppender}" Name="Card" Level="Debug"></log:LogConfig>
    </log:LogConfigs>
</ResourceDictionary>
