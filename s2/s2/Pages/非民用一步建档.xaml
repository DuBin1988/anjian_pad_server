<UserControl
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    xmlns:a="clr-namespace:Com.Aote.Attachs;assembly=s2DLL"
    xmlns:o="clr-namespace:Com.Aote.ObjectTools;assembly=s2DLL"
    xmlns:b="clr-namespace:Com.Aote.Behaviors;assembly=s2DLL"
	xmlns:c="clr-namespace:Com.Aote.Controls;assembly=s2DLL"
    xmlns:m="clr-namespace:Com.Aote.Marks;assembly=s2DLL"
    xmlns:i="http://schemas.microsoft.com/expression/2010/interactivity"
    xmlns:ei="http://schemas.microsoft.com/expression/2010/interactions"
    xmlns:data="clr-namespace:System.Windows.Data;assembly=System.Windows"
    xmlns:sdk="http://schemas.microsoft.com/winfx/2006/xaml/presentation/sdk"
    xmlns:toolkit="http://schemas.microsoft.com/winfx/2006/xaml/presentation/toolkit"
	xmlns:System="clr-namespace:System;assembly=mscorlib"
	mc:Ignorable="d" FontSize="13"
	x:Class="Com.Aote.Pages.非民用一步建档" FontFamily="Microsoft YaHei">
<UserControl.Resources>
		<LinearGradientBrush x:Key="TitleBgColorBrush1" EndPoint="0.5,1" StartPoint="0.5,0">
			<GradientStop Color="#FF9FCDFF" Offset="1"/>
			<GradientStop Color="#FFD4EEFF" Offset="0.004"/>
		</LinearGradientBrush>
	</UserControl.Resources>
	<Grid x:Name="LayoutRoot">
		<m:ResourceLoad Margin="0,0,-101,0">
			<!--单位对象 -->
        	<o:GeneralObject Name="units"  EntityType="t_unit" WebClientInfo="{StaticResource dbclient}"
			 Path="{m:Exp Str=$one/from t_unit where f_unitid\=\'\{unitinfos.SelectedItem.f_unitid\}\'}" >	
			</o:GeneralObject>
			<!--单位列表 -->
			 
							<o:PagedObjectList Name="unitinfoslist" WebClientInfo="{StaticResource dbclient}"
								Path="{m:Exp Str=$from t_unit where \{unitinfosearch.DataContext.Condition\}  }"
								IsOld="{m:Exp Str=SaveAction.State\=\=$End$ or userfile.DataContext.State\=\=$End$}"
								PageIndex="{m:Exp Str=pager.PageIndex}" PageSize="{m:Exp Str=pager.PageSize}" SumNames="yi"/>
						 
			
			 
			<!--气表品牌列表-->
			<o:ObjectList Name="pinpailist" WebClientInfo="{StaticResource dbclient}" Path="{m:Exp Str=$from qibiao as qibiao left join fetch qibiao.qbnumber}"/>
		   <!--用气性质列表-->
			<o:ObjectList Name="usegaslist" WebClientInfo="{StaticResource dbclient}" Path="{m:Exp Str=$from gasproper as gasproper left join fetch gasproper.pricetype where f_gasproperties\=\'非民用\' or f_gasproperties\=\'混合型\'}"/>
			<!--用户等级列-->
			<o:ObjectList Name="userpriviouslist" WebClientInfo="{StaticResource dbclient}" Path="from paramvalue where param.name='用户等级' order by id"/>
			<!--用途选择列-->
			<o:ObjectList Name="usetypelist" WebClientInfo="{StaticResource dbclient}" Path="from paramvalue where param.name='用途选择' order by id"/>
			
			<!--单值用户编号对象ksnumobj.value-->
			<o:GeneralObject Name="ksnumobj" EntityType="t_singlevalue" WebClientInfo="{StaticResource dbclient}" 
				Path="{m:Exp Str=$one/from t_singlevalue where name \= \'\{userfile.DataContext.f_filiale\}\用\户\编\号\'}" 
				IsInit="{m:Exp Str='SaveAction.State==$End'}"  >
			</o:GeneralObject>
				
			<!--单值用户编号对象,用于保存时获取用户编号-->
			<o:GeneralObject Name="ksnumobj1" EntityType="t_singlevalue" WebClientInfo="{StaticResource dbclient}" 
				LoadOnPathChanged="False"  Path="{m:Exp Str=$sernum/from t_singlevalue where name \= \'\{userfile.DataContext.f_filiale\}\用\户\编\号\' /value$}" 
			>
			</o:GeneralObject>
			
			<!--打开子窗口对象-->
			<o:ChildWindowObj Name="openkafei" CWName="/Pages/高级管理员登录"/>
			<!--单值对象-->
			<o:GeneralObject Name="dkfobj" EntityType="t_singlevalue" WebClientInfo="{StaticResource dbclient}" 
		  		Path="one/from t_singlevalue where name='大卡发卡费' "   ></o:GeneralObject>
			<o:GeneralObject Name="xkfobj" EntityType="t_singlevalue" WebClientInfo="{StaticResource dbclient}" 
		  		Path="one/from t_singlevalue where name='小卡发卡费' "   ></o:GeneralObject>
			<o:GeneralObject Name="mzdgasobj" EntityType="t_singlevalue" WebClientInfo="{StaticResource dbclient}" 
		  		Path="one/from t_singlevalue where name='民用单次最大购气量'"   ></o:GeneralObject>
			<o:GeneralObject Name="fzdgasobj" EntityType="t_singlevalue" WebClientInfo="{StaticResource dbclient}" 
		  		Path="one/from t_singlevalue where name='非民用单次最大购气量'"   ></o:GeneralObject>
			<!--混合气价对象 -->
        	<o:GeneralObject Name="hunhes" EntityType="t_hunhe" WebClientInfo="{StaticResource dbclient}"
             Path="{m:Exp Str=$one/from t_hunhe }">
        		
			</o:GeneralObject>
			<!--用气设备信息-->
			<o:ObjectList Name="devicelist" EntityType="t_devices" HasEmptyRow="True"></o:ObjectList>

		<!--用户对象-->
		<o:GeneralObject Name="userfilego" WebClientInfo="{StaticResource dbclient}" 
			EntityType="t_userfiles"
			IsInit="{m:Exp Str=SaveAction.State\=\=$End$}" >
			<o:PropertySetter PropertyName="f_zhye" Default="0"/>
			<o:PropertySetter PropertyName="f_state" Default="正常"/>
			<o:PropertySetter  PropertyName="f_substate" Default="完成"/>
			<o:PropertySetter PropertyName="f_device" Default="{m:Exp Str=devicelist}"></o:PropertySetter>
		</o:GeneralObject>

		 <b:SyncActionFactory Name="SaveAction"   >
		     <!--保存数据-->
		    <b:CallMethodAction    Name="saveu" TargetObject="{m:Exp Str=userfile.DataContext}"    MethodName="Save"/>  
         
		 </b:SyncActionFactory>

		</m:ResourceLoad>
		<Grid>
			<Grid.ColumnDefinitions>
	    		<ColumnDefinition Width="0.25*"/>
	    		<ColumnDefinition Width="0.75*"/>
    		</Grid.ColumnDefinitions>
			<c:BusyIndicatorEx BusyContent="正在查询，请稍等" IsBusy="{m:Exp Str=unitinfoslist.IsBusy}" >
				<toolkit:DockPanel >
					<sdk:DataPager x:Name="pager" Source="{Binding ItemsSource.Count, Converter={StaticResource DataPagerConverter}, ElementName=unitinfos}" PageSize="20" Height="24" VerticalAlignment="Top" toolkit:DockPanel.Dock="Bottom"/>
					<Border BorderThickness="1" BorderBrush="#FF9AA2AB" Margin="0" Padding="2" Width="Auto" Height="Auto" VerticalAlignment="Stretch" toolkit:DockPanel.Dock="Top" >
						<toolkit:DockPanel Width="Auto"   >
							<toolkit:DockPanel   Background="{x:Null}" toolkit:DockPanel.Dock="Bottom" Margin="0,0,0,5" Height="22">
								<TextBlock TextWrapping="Wrap" Text="{m:Exp Str=$共\{unitinfos.ItemsSource.Count\}项}" Width="Auto" HorizontalAlignment="Right" Foreground="#FF140000" VerticalAlignment="Bottom" Margin="0,0,0,1"/>
							</toolkit:DockPanel>
							<toolkit:DockPanel  x:Name="unitinfosearch" toolkit:DockPanel.Dock="Top"  Margin="0,0,0,5" Background="#FFEAF8FF"  >
								<a:Data.Context>
									<o:SearchObject />        	
								</a:Data.Context>
								<Grid>
									<Grid.ColumnDefinitions>
										<ColumnDefinition Width="70"/>
										<ColumnDefinition Width="50*"/>
										<ColumnDefinition Width="50*"/>
									</Grid.ColumnDefinitions>
									<Grid.RowDefinitions>
										<RowDefinition Height="28"/>
										<RowDefinition Height="28"/>
									</Grid.RowDefinitions>
									<TextBlock TextWrapping="Wrap" Text="单位编号：" Margin="5,4,0,4" />
									<TextBox TextWrapping="Wrap" Grid.Column="1" Text="{m:Bind Path=f_unitid}" Width="Auto" Margin="0" Height="25" >
										<a:Data.Prop>
											<o:PropertySetter PropertyName="f_unitid"  Operator="f_unitid like 'this%'"/>          
										</a:Data.Prop>
									</TextBox>
									<TextBlock TextWrapping="Wrap" Grid.Row="1" Text="单位名称：" Margin="5,4,0,4" />
									<TextBox TextWrapping="Wrap" Grid.Row="1" Grid.Column="1" Text="{m:Bind Path=f_unitname}" Width="Auto" Margin="0" Height="25" >
										<a:Data.Prop>
											<o:PropertySetter PropertyName="f_unitname"  Operator="f_unitname like 'this%'"/>          
										</a:Data.Prop>
									</TextBox>
									<Button Content="查  询" x:Name="unitsearchbutton" Grid.Column="2" Grid.Row="1" Click="{m:Exp Str=unitinfosearch.DataContext.Search()}" Margin="0,0,5,-2" HorizontalAlignment="Right" d:LayoutOverrides="Height" Width="50"/>
								</Grid>
							</toolkit:DockPanel>
							<sdk:DataGrid x:Name="unitinfos" Margin="0"  ItemsSource="{m:Exp Str=unitinfoslist}"
								AutoGenerateColumns="False" Width="Auto" Background="{x:Null}" IsReadOnly="True"  >
								<sdk:DataGrid.Columns>
									<sdk:DataGridTextColumn d:IsLocked="True" Header="序号" Binding="{Binding Index}"/>
									<sdk:DataGridTextColumn Header="单位名称" Binding="{Binding f_unitname}"/>
									<sdk:DataGridTextColumn Header="单位编号" Binding="{Binding f_unitid}"/>
									<sdk:DataGridTextColumn Header="地址" Binding="{Binding f_address}"/>
									<sdk:DataGridTextColumn Header="门牌号" Binding="{Binding f_apartment}"/>
									<sdk:DataGridTextColumn Header="联系人" Binding="{Binding f_linkname}"/>
									<sdk:DataGridTextColumn Header="电话" Binding="{Binding f_phone}"/>
								</sdk:DataGrid.Columns>
							</sdk:DataGrid>
										
						</toolkit:DockPanel>
					</Border>
					<!--提交按钮-->
				</toolkit:DockPanel >
			</c:BusyIndicatorEx>
			<c:BusyIndicatorEx BusyContent="正在工作，请稍等" IsBusy="{m:Exp Str=userfile.DataContext.IsBusy or SaveAction.IsBusy}" Margin="5,0,0,0" Grid.Column="1">
				<Grid  x:Name="userfile" DataContext="{m:Exp Str=userfilego}">
					<toolkit:DockPanel>
					<!--按钮-->
						<StackPanel  HorizontalAlignment="Center" toolkit:DockPanel.Dock="Bottom" Orientation="Horizontal" >
							<Button x:Name="save" Content="保  存" Margin="5" 
								IsEnabled="{m:Exp Str=!userfile.DataContext.HasErrors }" 
								Click="{m:Exp Str=SaveAction.Invoke()}" Width="50" />
							<Button Content="撤  销" Margin="5" Click="{m:Exp Str=userfile.DataContext.New()}" Width="50"/>
						</StackPanel>
						<ScrollViewer HorizontalScrollBarVisibility="Auto" VerticalScrollBarVisibility="Auto" toolkit:DockPanel.Dock="Top">
							<toolkit:DockPanel>
								<Grid toolkit:DockPanel.Dock="Top" Margin="0" HorizontalAlignment="Stretch" >
								<Grid.Background>
									<LinearGradientBrush EndPoint="0.5,1" MappingMode="RelativeToBoundingBox" StartPoint="0.5,0">
										<GradientStop Color="#FFF2F8FF" Offset="0"/>
										<GradientStop Color="#FFDDF2FD" Offset="1"/>
									</LinearGradientBrush>
								</Grid.Background>
								<Grid.RowDefinitions>
									<RowDefinition Height="20"/>
									<RowDefinition Height="30"/>
									<RowDefinition Height="30"/>
									<RowDefinition Height="30"/>
									<RowDefinition Height="30"/>
									
									<RowDefinition Height="30"/>
									<RowDefinition Height="30"/>
									<RowDefinition Height="30"/>
									<RowDefinition Height="30"/>
									<RowDefinition Height="30"/>
									
									<RowDefinition Height="30"/>
									<RowDefinition Height="30"/>
									<RowDefinition Height="30"/>
									<RowDefinition Height="30"/>
									<RowDefinition Height="30"/>
									
									<RowDefinition Height="30"/>
									<RowDefinition Height="30"/>
									<RowDefinition Height="20"/>
									<RowDefinition Height="30"/>
									<RowDefinition Height="30"/>
									
									<RowDefinition Height="30"/>
									<RowDefinition Height="30"/>
									<RowDefinition Height="30"/>
									<RowDefinition Height="30"/>
									<RowDefinition Height="20"/>
									
									<RowDefinition Height="30"/>
									<RowDefinition Height="30"/>
									<RowDefinition Height="1"/>
									<RowDefinition Height="1"/>
									<RowDefinition Height="1"/>
								</Grid.RowDefinitions>
								<Grid.ColumnDefinitions>
									<ColumnDefinition Width="130"/>
									<ColumnDefinition Width="1*"/>
									<ColumnDefinition Width="130"/>
									<ColumnDefinition Width="1*"/>
									<ColumnDefinition Width="110"/>
									<ColumnDefinition Width="1*"/>
								</Grid.ColumnDefinitions>
								<TextBlock Text="单位名称：" Grid.Column="2" Margin="0,2,0,3" TextWrapping="Wrap" TextAlignment="Right" Height="25" Grid.Row="2" HorizontalAlignment="Right" Width="65"/>
								<TextBlock Text="联 系 人：" Margin="-1,2,1,3" TextWrapping="Wrap" TextAlignment="Right" Grid.Row="4" Height="25"/>
								<TextBlock Text="地　　址：" Margin="44,2,0,3" TextWrapping="Wrap" TextAlignment="Right" Height="25" Grid.Row="5" Grid.Column="4"/>
								<TextBlock Text="用气规模：" Margin="49,1,0,4" TextWrapping="Wrap" TextAlignment="Right" Height="25" Grid.Row="8"/>
								<TextBlock Text="证件类型：" Margin="63,3,2,2" TextWrapping="Wrap" TextAlignment="Right" Height="25" Grid.Row="3"/>
								<TextBlock Text="电　　话：" Margin="40,1,0,4" TextWrapping="Wrap" TextAlignment="Right" Grid.Column="4" Grid.Row="3" Height="25"/>
								<TextBlock Text="用户等级：" Margin="-20,3,0,2" TextWrapping="Wrap" TextAlignment="Right" Height="25" Grid.Row="4" Grid.Column="4"/>
								<TextBlock Text="卡    费：" Margin="0,2,0,3" TextWrapping="Wrap" TextAlignment="Right" Height="25" Grid.Row="9" Grid.Column="2"/>
								<TextBlock Text="折    费：" Margin="-20,3,0,2" TextWrapping="Wrap" TextAlignment="Right" Height="25" Grid.Row="9" Grid.Column="4"/>
								<TextBlock Text="用气押金：" Margin="61,2,0,3" TextWrapping="Wrap" TextAlignment="Right" Grid.Row="8" Height="25" Grid.Column="2"/>
								<TextBlock Text="安装地点：" Margin="0,9,0,2" TextWrapping="Wrap" TextAlignment="RIGHT" Grid.Row="25" Grid.Column="5" HorizontalAlignment="Left" Width="65" d:LayoutOverrides="HorizontalAlignment"/>
								<TextBlock Text="用途选择：" Margin="-20,3,0,2" TextWrapping="Wrap" TextAlignment="Right" Grid.Row="11" Height="25" Grid.Column="4"/>
								<TextBlock Text="用户类型：" Margin="61,2,0,3" TextWrapping="Wrap" TextAlignment="Right" Grid.Row="5" Height="25"/>
								<TextBlock Text="用气性质：" Margin="19,3,0,2" Grid.Row="11" TextWrapping="Wrap" TextAlignment="Right" Height="25"/>
								<TextBlock Text="气价类型：" Grid.Column="2" Margin="0,3,0,2" Grid.Row="11" TextWrapping="Wrap" TextAlignment="Right" Height="25" HorizontalAlignment="Right" Width="69"/>
								<TextBlock Text="气　　价：" Margin="64,3,0,2" Grid.Row="13" TextWrapping="Wrap" TextAlignment="Right" Height="25"/>
							 
								<Border Margin="0" BorderThickness="0,0,0,0" Height="20" Grid.ColumnSpan="6" Grid.Row="17" d:LayoutOverrides="VerticalAlignment" CornerRadius="5,5,0,0" Background="{StaticResource TitleBgColorBrush1}" />
								<TextBlock Text="表具信息：" Margin="15,0,0,0"  Height="20" Grid.ColumnSpan="6" Grid.Row="17" Foreground="Black" VerticalAlignment="Center" FontWeight="Bold" ></TextBlock>
								
							 	 	<Border  Grid.Row="0" BorderThickness="0"  Grid.ColumnSpan="6" HorizontalAlignment="Stretch" d:LayoutOverrides="VerticalAlignment" CornerRadius="5,5,0,0" Background="{StaticResource TitleBgColorBrush1}" Height="20" />
							<TextBlock Text="基本信息:" Grid.Column="0" Grid.ColumnSpan="6" Foreground="Black" Margin="0,0,15,0" FontWeight="Bold"/>
								
								<Border  Grid.Row="24" Margin="0" BorderThickness="0,0,0,0"  Grid.ColumnSpan="6" Height="20" VerticalAlignment="Bottom" Background="{StaticResource TitleBgColorBrush1}" >
								 
								</Border>
								<TextBlock Grid.Row="24" Margin="15,2,0,0" Text="设备信息：" Grid.ColumnSpan="6" HorizontalAlignment="Stretch" Foreground="Black" FontWeight="Bold" d:LayoutOverrides="Height"/>
								<Border  Grid.Row="28" Margin="5,0,0,0" BorderThickness="0,0,0,3"  Grid.ColumnSpan="6" >
									<Border.Background>
										<LinearGradientBrush EndPoint="0.5,1" StartPoint="0.5,0">
											<GradientStop Color="#FFC6E2FB" Offset="1"/>
											<GradientStop Color="#FFE6F1FB"/>
										</LinearGradientBrush>
									</Border.Background>
								</Border>
								<TextBox Text="{m:Bind Path=f_username}" Grid.Column="3" LostFocus="{m:Exp Str=ksnumobj.Load()}" Margin="0,2,0,3" Grid.Row="2" TextWrapping="Wrap" Grid.ColumnSpan="1" Height="25" VerticalAlignment="Stretch" Grid.RowSpan="1" IsReadOnly="True" >
									<a:Data.Prop>
										<o:PropertySetter PropertyName="f_username" Value="{m:Exp Str=unitinfos.SelectedItem.f_unitname}"/>
									</a:Data.Prop>
								</TextBox>	
								
								<TextBox Text="{m:Bind Path=f_linkname}"  Grid.Column="1" Margin="0,2,0,3" Grid.Row="4" TextWrapping="Wrap" Height="25" VerticalAlignment="Stretch" RenderTransformOrigin="-1.313,0.4" Grid.RowSpan="1"  >
									<a:Data.Prop>
										<o:PropertySetter PropertyName="f_linkname" NotNull="True" Value="{m:Exp Str=unitinfos.SelectedItem.f_linkname}"/>
									</a:Data.Prop>
								</TextBox>	
							
								<TextBox Text="{m:Bind Path=f_address}" IsReadOnly="True" Grid.Column="5" Margin="0,1,0,4" Grid.Row="5" TextWrapping="Wrap" Grid.ColumnSpan="1" Height="25" VerticalAlignment="Stretch" HorizontalAlignment="Stretch" Width="Auto" >
									<a:Data.Prop>
										<o:PropertySetter PropertyName="f_address"  Value="{m:Exp Str=unitinfos.SelectedItem.f_address}"/>
									</a:Data.Prop>
								</TextBox>	
							
								<TextBox Text="{m:Bind Path=f_idnumber}"  Grid.Column="3" Margin="0,2,0,3" Grid.Row="3" TextWrapping="Wrap" IsReadOnly="False" Height="25" VerticalAlignment="Stretch" RenderTransformOrigin="0.368,-0.68" HorizontalAlignment="Stretch" Width="Auto" Grid.ColumnSpan="1"  >
									<a:Data.Prop>
										<o:PropertySetter PropertyName="f_idnumber" Value="{m:Exp Str=unitinfos.SelectedItem.f_idnumber}"/>
									</a:Data.Prop>
								</TextBox>	
								<TextBox Text="{m:Bind Path=f_phone}"  Grid.Column="5" Margin="0,2,0,3" Grid.Row="3" TextWrapping="Wrap" IsReadOnly="False" Height="25" VerticalAlignment="Stretch" RenderTransformOrigin="-1.435,0.56" Grid.RowSpan="1" >
									<a:Data.Prop>
										<o:PropertySetter PropertyName="f_phone" NotNull="True" Value="{m:Exp Str=unitinfos.SelectedItem.f_phone}"/>
									</a:Data.Prop>
								</TextBox>
								<TextBox Text="{m:Bind Path=f_weizhi}"  Grid.Column="5" Margin="0" Grid.Row="26" TextWrapping="Wrap" IsReadOnly="False" Height="25" VerticalAlignment="Stretch" Grid.ColumnSpan="1" HorizontalAlignment="Stretch" Width="Auto" >
								</TextBox>
								<TextBox Text="{m:Bind Path=f_gasdeposit}" Grid.Column="3" Margin="0,3,0,2" Grid.Row="8" IsReadOnly="False" Height="25" VerticalAlignment="Stretch" Grid.ColumnSpan="1" >
									<a:Data.Prop>
										<o:PropertySetter PropertyName="f_gasdeposit" Default="50"/>
									</a:Data.Prop>	
								</TextBox>	
								<TextBox Text="{m:Bind Path=f_gasscale}" Grid.Column="1" Margin="0,1,0,4" Grid.Row="8" IsReadOnly="False" Height="25" VerticalAlignment="Stretch" Grid.ColumnSpan="1" >
									<a:Data.Prop>
										<o:PropertySetter PropertyName="f_gasscale" Default="50"/>
									</a:Data.Prop>	
								</TextBox>	
								<TextBox Text="{m:Bind Path=f_usertype}" Grid.Column="1" Margin="0,3,0,2" Grid.Row="5" IsReadOnly="True" Height="25" VerticalAlignment="Stretch" Grid.ColumnSpan="1" >
									<a:Data.Prop>
										<o:PropertySetter PropertyName="f_usertype" Default="非民用"/>
									</a:Data.Prop>	
								</TextBox>	
								<TextBox Text="{m:Bind Path=f_xiangou}" Grid.Column="5" Margin="0,3,0,2" Grid.Row="8"  Height="25" VerticalAlignment="Stretch" >
									<a:Data.Prop>
										<o:PropertySetter PropertyName="f_xiangou"  NotNull="True"/>
									</a:Data.Prop>	
								</TextBox>	
								
								<ComboBox x:Name="ComboBox_usetype"  DisplayMemberPath="name" SelectedValuePath="name" ItemsSource="{m:Exp Str=usetypelist}" SelectedValue="{m:Bind Path=f_usetype}" Grid.Column="5" Margin="0,3,0,2" Grid.Row="11" Height="25" IsEnabled="true" HorizontalAlignment="Stretch" VerticalAlignment="Stretch" Grid.ColumnSpan="1" FontSize="13.333">
									 <a:Data.Prop>
									     <o:PropertySetter PropertyName="f_usetype" NotNull="True" ></o:PropertySetter>
								    </a:Data.Prop> 
								</ComboBox>
								<ComboBox x:Name="CoboxGasPro" ItemsSource="{m:Exp Str=usegaslist}" DisplayMemberPath="f_gasproperties" SelectedValuePath="f_gasproperties" SelectedValue="{m:Bind Path=f_gasproperties}"  Grid.Column="1" Margin="-1,3,0,2" Grid.Row="11" Height="25" >
									<!--a:Data.Items>
										<o:ObjectList WebClientInfo="{StaticResource dbclient}" Path="{m:Exp Str=$from gasproper as gasproper left join fetch gasproper.pricetype where f_gasproperties\=\'非民用\' or f_gasproperties\=\'混合型\'}"/>
									</a:Data.Items-->
									<a:Data.Prop>
										<o:PropertySetter PropertyName="f_gasproperties" NotNull="True" Value="{m:Exp Str=CoboxGasPro.SelectedItem.f_gasproperties}" />
									</a:Data.Prop>		
								</ComboBox>
								<ComboBox x:Name="CoboxGas" DisplayMemberPath="f_gaspricetype" SelectedValuePath="f_gaspricetype" ItemsSource="{m:Exp Str=CoboxGasPro.SelectedItem.pricetype}" SelectedValue="{m:Bind Path=f_gaspricetype}"  Grid.Column="3" Margin="0,3,0,2" Grid.Row="11" Height="25" VerticalAlignment="Stretch" >
								    <a:Data.Prop>
										<o:PropertySetter PropertyName="f_gaspricetype" NotNull="True" Value="{m:Exp Str=CoboxGasPro.SelectedItem.f_gaspricetype}" ></o:PropertySetter>
									</a:Data.Prop>
								</ComboBox>
								<TextBox Text="{m:Bind Path=f_gasprice}" Grid.Column="1" Margin="0,2,0,3" Grid.Row="13" TextWrapping="Wrap" IsReadOnly="False" Height="25" VerticalAlignment="Stretch">
									<a:Data.Prop>
										<o:PropertySetter PropertyName="f_gasprice" Value="{m:Exp Str=CoboxGas.SelectedItem.f_gasprice}"  />
									</a:Data.Prop>
								</TextBox>
								<TextBlock  Text="混合气价：" Margin="0,5,0,0" Grid.Row="12" Height="25" TextAlignment="Right"  />
	
								<TextBlock Grid.Column="0" Margin="0,2,0,3" Grid.Row="20" TextWrapping="Wrap" Text="表　　号：" TextAlignment="Right" Height="25"/>
								<TextBlock Margin="0,2,0,3" Grid.Row="23" TextWrapping="Wrap" Text="大 小 卡：" TextAlignment="Right" Height="25"/>
