<UserControl
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    xmlns:a="clr-namespace:Com.Aote.Attachs;assembly=s2DLL"
    xmlns:o="clr-namespace:Com.Aote.ObjectTools;assembly=s2DLL"
    xmlns:b="clr-namespace:Com.Aote.Behaviors;assembly=s2DLL"
	xmlns:c="clr-namespace:Com.Aote.Controls;assembly=s2DLL"
    xmlns:m="clr-namespace:Com.Aote.Marks;assembly=s2DLL"
    xmlns:i="http://schemas.microsoft.com/expression/2010/interactivity"
    xmlns:ei="http://schemas.microsoft.com/expression/2010/interactions"
    xmlns:data="clr-namespace:System.Windows.Data;assembly=System.Windows"
    xmlns:sdk="http://schemas.microsoft.com/winfx/2006/xaml/presentation/sdk"
    xmlns:toolkit="http://schemas.microsoft.com/winfx/2006/xaml/presentation/toolkit" 
	mc:Ignorable="d" FontSize="13"
	x:Class="Com.Aote.Pages.财务报表">
	<Grid>
		<m:ResourceLoad Margin="0,0,-12,0">
			<!--付款方式-->
			<o:ObjectList Name="fukuanfangshilist" WebClientInfo="{StaticResource dbclient}" Path="from paramvalue where param.name='付款方式' order by id"/>
			<!--网点列表对象-->
			<o:ObjectList Name="wangdianlist" WebClientInfo="{StaticResource dbclient}" Path="{m:Exp Str=$from paramvalue where param.name\=\'网点查询用\' and code \=\'\{infosearch.DataContext.f_filiale\}\'$}"
			IsOld="{m:Exp Str=fgslist.State\=\=$End$}"/>
			<!--查询分公司参数-->
			<o:ObjectList Name="fgslist" WebClientInfo="{StaticResource dbclient}" Path="from paramvalue where param.name='分公司' order by id"/>
			<!--民用/职工气convert(varchar(50)\,convert(decimal(30\,2)\,sum(f_preamount))) as f_preamount\,-->
			<o:GeneralObject  Name="sellmin" WebClientInfo="{StaticResource dbclient}" 
				EntityType="t_sellinggas"  Path="{m:Exp Str=$one/select new Map(count(*) as count\,Sum(f_pregas) as pregas\,sum(cast(f_preamount  as big_decimal)) as preamount) from t_sellinggas where \{searchbutton.DataContext.Condition\} and f_payfeevalid\=\'有效\' and (f_gaspricetype\=\'民用气价\' or f_gaspricetype\=\'职工气价\')}"/>
		   <!--福利气-->
			<o:GeneralObject  Name="sellfuli" WebClientInfo="{StaticResource dbclient}" 
				EntityType="t_sellinggas"  Path="{m:Exp Str=$one/select new Map(count(*) as count\,Sum(f_pregas) as pregas\,sum(cast(f_preamount  as big_decimal)) as preamount) from t_sellinggas where \{searchbutton.DataContext.Condition\} and f_payfeevalid\=\'有效\' and f_gaspricetype\=\'福利气价\'}"/>
		   <!--营业气-->
			<o:GeneralObject  Name="sellyingye" WebClientInfo="{StaticResource dbclient}" 
				EntityType="t_sellinggas"  Path="{m:Exp Str=$one/select new Map(count(*) as count\,Sum(f_pregas) as pregas\,sum(cast(f_preamount  as big_decimal)) as preamount) from t_sellinggas where \{searchbutton.DataContext.Condition\} and f_payfeevalid\=\'有效\' and f_gaspricetype\=\'商业气价\'}"/>
		   <!--工业气-->
			<o:GeneralObject  Name="sellgongye" WebClientInfo="{StaticResource dbclient}" 
				EntityType="t_sellinggas"  Path="{m:Exp Str=$one/select new Map(count(*) as count\,Sum(f_pregas) as pregas\,sum(cast(f_preamount  as big_decimal)) as preamount) from t_sellinggas where \{searchbutton.DataContext.Condition\} and f_payfeevalid\=\'有效\' and f_gaspricetype\=\'工业气价\'}"/>
		   <!--麟游民用气-->
			<o:GeneralObject  Name="selllymin" WebClientInfo="{StaticResource dbclient}" 
				EntityType="t_sellinggas"  Path="{m:Exp Str=$one/select new Map(count(*) as count\,Sum(f_pregas) as pregas\,sum(cast(f_preamount  as big_decimal)) as preamount) from t_sellinggas where \{searchbutton.DataContext.Condition\} and f_payfeevalid\=\'有效\' and f_gaspricetype\=\'麟游民用气价\'}"/>
		   <!--麟游营业气-->
			<o:GeneralObject  Name="selllyyingye" WebClientInfo="{StaticResource dbclient}" 
				EntityType="t_sellinggas"  Path="{m:Exp Str=$one/select new Map(count(*) as count\,Sum(f_pregas) as pregas\,sum(cast(f_preamount  as big_decimal)) as preamount) from t_sellinggas where \{searchbutton.DataContext.Condition\} and f_payfeevalid\=\'有效\' and f_gaspricetype\=\'麟游营业气价\'}"/>
		   <!--维护费统计-->
			<o:GeneralObject  Name="selllweihu" WebClientInfo="{StaticResource dbclient}" 
				EntityType="t_sellinggas"  Path="{m:Exp Str=$one/select new Map(count(*) as count\,Sum(f_amountmaintenance) as amountmaintenance) from t_sellinggas where \{searchbutton.DataContext.Condition\} and f_payfeevalid\=\'有效\' and (f_amountmaintenance\!\=\null or f_amountmaintenance\>\=\0)}"/>
		</m:ResourceLoad>
		<toolkit:BusyIndicator BusyContent="正在查询，请稍等" IsBusy="{m:Exp Str=sellmin.IsBusy or sellfuli.IsBusy or sellyingye.IsBusy or sellgongye.IsBusy or selllymin.IsBusy or selllyyingye.IsBusy or selllweihu.IsBusy}" >
		<toolkit:DockPanel >
			<!--民用气价查询条件-->
			<toolkit:DockPanel x:Name="infosearch"  toolkit:DockPanel.Dock="Top" Margin="0" Background="#FFCFEFFF" d:LayoutOverrides="Width" >
				<a:Data.Context>
					<o:SearchObject />        	
				</a:Data.Context>
				<StackPanel Orientation="Horizontal">
					<TextBlock TextWrapping="Wrap" Text="发票号 从：" Margin="5,6,0,6" Height="25"/>
					<TextBox Text="{m:Bind Path=f_invoicenum1}" Margin="0,6"  Height="25" VerticalAlignment="Stretch" Width="35"  >
						<a:Data.Prop>
								<o:PropertySetter PropertyName="f_invoicenum1"  Operator="f_invoicenum >= this"/>               
							</a:Data.Prop>
					</TextBox>
					<TextBlock TextWrapping="Wrap" Text="到：" Margin="5,6,0,6" Height="25"/>
					<TextBox Text="{m:Bind Path=f_invoicenum2}" Margin="0,6"  Height="25" VerticalAlignment="Stretch" Width="35"  >
						<a:Data.Prop>
								<o:PropertySetter PropertyName="f_invoicenum2"  Operator="f_invoicenum ^= this"/>               
							</a:Data.Prop>
					</TextBox>
					
					<TextBlock TextWrapping="Wrap" Text="开始日期：" Margin="5,6,0,6" Height="25"/>
					<sdk:DatePicker SelectedDate="{m:Bind Path=f_startedate}" Margin="0,6"  Height="25" VerticalAlignment="Stretch" Width="120"  >
						<a:Data.Prop>
							<o:PropertySetter PropertyName="f_startedate" Value="{m:Exp Str=SysTime.Today}"
								Operator=" f_deliverydate >= substring('this',1,10)"/>
						</a:Data.Prop>
					</sdk:DatePicker>
					<TextBlock TextWrapping="Wrap" Text="结束日期：" Margin="5,6,0,6" Height="25"/>
					<sdk:DatePicker SelectedDate="{m:Bind Path=f_enddate}" Margin="0,6"  Height="25" VerticalAlignment="Stretch" Width="120"  >
						<a:Data.Prop>
							<o:PropertySetter PropertyName="f_enddate" Value="{m:Exp Str=SysTime.Today}"
								Operator=" f_deliverydate ^= substring('this',1,10)"/>
						</a:Data.Prop>
					</sdk:DatePicker>	
										<TextBlock TextWrapping="Wrap" Text="付款方式：" Margin="5,6,0,6" Height="25"/>
						
						<ComboBox Name="fukuanfangshi"   SelectedValue="{m:Bind Path=f_payment}" Grid.Row="1" Grid.Column="3" ItemsSource="{m:Exp Str=fukuanfangshilist}" SelectedValuePath="name"  Margin="0" DisplayMemberPath="name"   Height="25" >
							
							<a:Data.Prop>
                    		<o:PropertySetter PropertyName="f_payment"  Operator="f_payment = 'this'"/>          
                    	    </a:Data.Prop>
						</ComboBox>
					<TextBlock TextWrapping="Wrap" Text="分公司：" Margin="5,6,0,6" Height="25"/>
						
						<ComboBox Name="Comboxfx"   SelectedValue="{m:Bind Path=f_filiale}" Grid.Row="1" Grid.Column="3" ItemsSource="{m:Exp Str=fgslist}"  Margin="0" DisplayMemberPath="name" SelectedValuePath="name"  Height="25" Width="50">
							<a:Data.Prop>
                    		<o:PropertySetter PropertyName="f_filiale"  Operator="f_filiale = 'this'"/>          
                    	</a:Data.Prop>
						</ComboBox>
					<TextBlock TextWrapping="Wrap" Text="网点：" Margin="5,6,0,6" Height="25"/>
						
						<ComboBox Name="wangdian"   SelectedValue="{m:Bind Path=f_sgnetwork}" Grid.Row="1" Grid.Column="3" ItemsSource="{m:Exp Str=wangdianlist}"  Margin="0" DisplayMemberPath="name" SelectedValuePath="name"  Height="25" Width="50">
							<a:Data.Prop>
                    		<o:PropertySetter PropertyName="f_sgnetwork"  Operator="f_sgnetwork = 'this'"/>          
                    	</a:Data.Prop>
						</ComboBox>
				</StackPanel> 		
				<Button Content="查  询" x:Name="searchbutton" Click="{m:Exp Str=infosearch.DataContext.Search();infosearch1.DataContext.Search();infosearch2.DataContext.Search();infosearch3.DataContext.Search();infosearch4.DataContext.Search();infosearch5.DataContext.Search();infosearch6.DataContext.Search()}" Margin="10,0,5,5" HorizontalAlignment="Left" Width="50"  Height="32" Padding="2" />
				<Button Content="置  空" x:Name="zhikong" Click="{m:Exp Str=infosearch.DataContext.f_invoicenum1\=null;infosearch.DataContext.f_invoicenum2\=null;infosearch.DataContext.f_startedate\=null;infosearch.DataContext.f_enddate\=null;Comboxfx.SelectedValue\=null;wangdian.SelectedValue\=null;fukuanfangshi.SelectedValue\=null}" Margin="0,0,5,5" HorizontalAlignment="Left" Width="50"  Height="32" Padding="2" RenderTransformOrigin="0.14,0.469" />
				<Button Content="打  印" x:Name="zhikong_Copy" Click="{m:Exp Str=print.Print()}" Margin="0,0,5,5" HorizontalAlignment="Left" Width="50"  Height="32" Padding="2" RenderTransformOrigin="0.14,0.469" />
			</toolkit:DockPanel>
			<!--福利气价查询条件-->
			<toolkit:DockPanel x:Name="infosearch1"  toolkit:DockPanel.Dock="Top" Margin="0" Background="#FFCFEFFF" d:LayoutOverrides="Width" Height="35" Visibility="Collapsed" >
				<a:Data.Context>
					<o:SearchObject />        	
				</a:Data.Context>
				<StackPanel Orientation="Horizontal">
					<TextBlock TextWrapping="Wrap" Text="发票号 从：" Margin="5,6,0,6" Height="25"/>
					<TextBox Text="{m:Bind Path=f_invoicenum1}" Margin="0,6"  Height="25" VerticalAlignment="Stretch" Width="80"  >
						<a:Data.Prop>
								<o:PropertySetter PropertyName="f_invoicenum1" Value="{m:Exp Str=infosearch.DataContext.f_invoicenum1}" Operator="f_invoicenum >= this"/>               
							</a:Data.Prop>
					</TextBox>
					<TextBlock TextWrapping="Wrap" Text="到：" Margin="5,6,0,6" Height="25"/>
					<TextBox Text="{m:Bind Path=f_invoicenum2}" Margin="0,6"  Height="25" VerticalAlignment="Stretch" Width="80"  >
						<a:Data.Prop>
								<o:PropertySetter PropertyName="f_invoicenum2" Value="{m:Exp Str=infosearch.DataContext.f_invoicenum2}" Operator="f_invoicenum ^= this"/>               
							</a:Data.Prop>
					</TextBox>
					
					
					<TextBlock TextWrapping="Wrap" Text="日期：" Margin="5,6,0,6" Height="25"/>
					<sdk:DatePicker SelectedDate="{m:Bind Path=f_startedatefuli}" Margin="0,6"  Height="25" VerticalAlignment="Stretch"  >
						<a:Data.Prop>
							<o:PropertySetter PropertyName="f_startedatefuli" Value="{m:Exp Str=infosearch.DataContext.f_startedate}"
								Operator=" f_deliverydate &gt;= substring('this',1,10)"/>
						</a:Data.Prop>
					</sdk:DatePicker>
					<TextBlock TextWrapping="Wrap" Text="日期：" Margin="5,6,0,6" Height="25"/>
					<sdk:DatePicker SelectedDate="{m:Bind Path=f_enddatefuli}" Margin="0,6"  Height="25" VerticalAlignment="Stretch"  >
						<a:Data.Prop>
							<o:PropertySetter PropertyName="f_enddatefuli" Value="{m:Exp Str=infosearch.DataContext.f_enddate}"
								Operator=" f_deliverydate ^= substring('this',1,10)"/>
						</a:Data.Prop>
					</sdk:DatePicker>	
					<TextBlock TextWrapping="Wrap" Text="分公司：" Margin="5,6,0,6" Height="25"/>
					<TextBox x:Name="Comboxfx1"   Text="{m:Bind Path=f_filiale}" Grid.Row="1" Grid.Column="3"  Margin="0"   Height="25" Width="180">
						<a:Data.Prop>
							<o:PropertySetter PropertyName="f_filiale" Value="{m:Exp Str=infosearch.DataContext.f_filiale}" Operator="f_filiale = 'this'"/>          
						</a:Data.Prop>
					</TextBox>
					<TextBlock TextWrapping="Wrap" Text="网点：" Margin="5,6,0,6" Height="25"/>
						<TextBox x:Name="wangdian1" Text="{m:Bind Path=f_sgnetwork}" Grid.Row="1" Grid.Column="3"   Margin="0"  Height="25" Width="200">
							<a:Data.Prop>
                    		<o:PropertySetter PropertyName="f_sgnetwork" Value="{m:Exp Str=infosearch.DataContext.f_sgnetwork}" Operator="f_sgnetwork = 'this'"/>          
                    	</a:Data.Prop>
						</TextBox>
					<Button Content="查  询" x:Name="searchbutton1" Click="{m:Exp Str=infosearch1.DataContext.Search()}" Margin="10,0,5,5" HorizontalAlignment="Left" Width="50"  Height="32" Padding="2" />			
				</StackPanel>
			</toolkit:DockPanel>
			<!--营业气价查询条件-->
			<toolkit:DockPanel x:Name="infosearch2"  toolkit:DockPanel.Dock="Top" Margin="0" Background="#FFCFEFFF" d:LayoutOverrides="Width" Height="35" Visibility="Collapsed" >
				<a:Data.Context>
					<o:SearchObject />        	
				</a:Data.Context>
				<StackPanel Orientation="Horizontal">
					<TextBlock TextWrapping="Wrap" Text="发票号 从：" Margin="5,6,0,6" Height="25"/>
					<TextBox Text="{m:Bind Path=f_invoicenum1}" Margin="0,6"  Height="25" VerticalAlignment="Stretch" Width="80"  >
						<a:Data.Prop>
								<o:PropertySetter PropertyName="f_invoicenum1" Value="{m:Exp Str=infosearch.DataContext.f_invoicenum1}" Operator="f_invoicenum >= this"/>               
							</a:Data.Prop>
					</TextBox>
					<TextBlock TextWrapping="Wrap" Text="到：" Margin="5,6,0,6" Height="25"/>
					<TextBox Text="{m:Bind Path=f_invoicenum2}" Margin="0,6"  Height="25" VerticalAlignment="Stretch" Width="80"  >
						<a:Data.Prop>
								<o:PropertySetter PropertyName="f_invoicenum2" Value="{m:Exp Str=infosearch.DataContext.f_invoicenum2}" Operator="f_invoicenum ^= this"/>               
							</a:Data.Prop>
					</TextBox>
					
					
					<TextBlock TextWrapping="Wrap" Text="换表日期：" Margin="5,6,0,6" Height="25"/>
					<sdk:DatePicker SelectedDate="{m:Bind Path=f_startedateyingye}" Margin="0,6"  Height="25" VerticalAlignment="Stretch"  >
						<a:Data.Prop>
							<o:PropertySetter PropertyName="f_startedateyingye" Value="{m:Exp Str=infosearch.DataContext.f_startedate}"
								Operator=" f_deliverydate &gt;= substring('this',1,10)"/>
						</a:Data.Prop>
					</sdk:DatePicker>
					<TextBlock TextWrapping="Wrap" Text="换表日期：" Margin="5,6,0,6" Height="25"/>
					<sdk:DatePicker SelectedDate="{m:Bind Path=f_enddateyingye}" Margin="0,6"  Height="25" VerticalAlignment="Stretch"  >
						<a:Data.Prop>
							<o:PropertySetter PropertyName="f_enddateyingye" Value="{m:Exp Str=infosearch.DataContext.f_enddate}"
								Operator=" f_deliverydate ^= substring('this',1,10)"/>
						</a:Data.Prop>
					</sdk:DatePicker>	
					<TextBlock TextWrapping="Wrap" Text="分公司：" Margin="5,6,0,6" Height="25"/>
						
					<TextBox x:Name="Comboxfx2"   Text="{m:Bind Path=f_filiale}" Grid.Row="1" Grid.Column="3"  Margin="0"   Height="25" Width="180">
						<a:Data.Prop>
							<o:PropertySetter PropertyName="f_filiale" Value="{m:Exp Str=infosearch.DataContext.f_filiale}" Operator="f_filiale = 'this'"/>          
						</a:Data.Prop>
					</TextBox>
					<TextBlock TextWrapping="Wrap" Text="网点：" Margin="5,6,0,6" Height="25"/>
						<TextBox x:Name="wangdian2" Text="{m:Bind Path=f_sgnetwork}" Grid.Row="1" Grid.Column="3"   Margin="0"  Height="25" Width="200">
							<a:Data.Prop>
                    		<o:PropertySetter PropertyName="f_sgnetwork" Value="{m:Exp Str=infosearch.DataContext.f_sgnetwork}" Operator="f_sgnetwork = 'this'"/>          
                    	</a:Data.Prop>
						</TextBox>
					<Button Content="查  询" x:Name="searchbutton2" Click="{m:Exp Str=infosearch2.DataContext.Search()}" Margin="10,0,5,5" HorizontalAlignment="Left" Width="50"  Height="32" Padding="2" />			
				</StackPanel>
			</toolkit:DockPanel>
			
			<!--工业气价查询条件-->
			<toolkit:DockPanel x:Name="infosearch3"  toolkit:DockPanel.Dock="Top" Margin="0" Background="#FFCFEFFF" d:LayoutOverrides="Width" Height="35" Visibility="Collapsed" >
				<a:Data.Context>
					<o:SearchObject />        	
				</a:Data.Context>
				<StackPanel Orientation="Horizontal">
					<TextBlock TextWrapping="Wrap" Text="发票号 从：" Margin="5,6,0,6" Height="25"/>
					<TextBox Text="{m:Bind Path=f_invoicenum1}" Margin="0,6"  Height="25" VerticalAlignment="Stretch" Width="80"  >
						<a:Data.Prop>
								<o:PropertySetter PropertyName="f_invoicenum1" Value="{m:Exp Str=infosearch.DataContext.f_invoicenum1}" Operator="f_invoicenum >= this"/>               
							</a:Data.Prop>
					</TextBox>
					<TextBlock TextWrapping="Wrap" Text="到：" Margin="5,6,0,6" Height="25"/>
					<TextBox Text="{m:Bind Path=f_invoicenum2}" Margin="0,6"  Height="25" VerticalAlignment="Stretch" Width="80"  >
						<a:Data.Prop>
								<o:PropertySetter PropertyName="f_invoicenum2" Value="{m:Exp Str=infosearch.DataContext.f_invoicenum2}" Operator="f_invoicenum ^= this"/>               
							</a:Data.Prop>
					</TextBox>
					
					<TextBlock TextWrapping="Wrap" Text="发卡日期：" Margin="5,6,0,6" Height="25"/>
					<sdk:DatePicker SelectedDate="{m:Bind Path=f_startedategongye}" Margin="0,6"  Height="25" VerticalAlignment="Stretch"  >
						<a:Data.Prop>
							<o:PropertySetter PropertyName="f_startedategongye" Value="{m:Exp Str=infosearch.DataContext.f_startedate}"
								Operator=" f_deliverydate &gt;= substring('this',1,10)"/>
						</a:Data.Prop>
					</sdk:DatePicker>
					<TextBlock TextWrapping="Wrap" Text="发卡日期：" Margin="5,6,0,6" Height="25"/>
					<sdk:DatePicker SelectedDate="{m:Bind Path=f_enddategongye}" Margin="0,6"  Height="25" VerticalAlignment="Stretch"  >
						<a:Data.Prop>
							<o:PropertySetter PropertyName="f_enddategongye" Value="{m:Exp Str=infosearch.DataContext.f_enddate}"
								Operator=" f_deliverydate ^= substring('this',1,10)"/>
						</a:Data.Prop>
					</sdk:DatePicker>	
					<TextBlock TextWrapping="Wrap" Text="分公司：" Margin="5,6,0,6" Height="25"/>
					<TextBox x:Name="Comboxfx3"   Text="{m:Bind Path=f_filiale}" Grid.Row="1" Grid.Column="3"  Margin="0"   Height="25" Width="180">
						<a:Data.Prop>
							<o:PropertySetter PropertyName="f_filiale" Value="{m:Exp Str=infosearch.DataContext.f_filiale}" Operator="f_filiale = 'this'"/>          
						</a:Data.Prop>
					</TextBox>
					<TextBlock TextWrapping="Wrap" Text="网点：" Margin="5,6,0,6" Height="25"/>
						<TextBox x:Name="wangdian3" Text="{m:Bind Path=f_sgnetwork}" Grid.Row="1" Grid.Column="3"   Margin="0"  Height="25" Width="200">
							<a:Data.Prop>
                    		<o:PropertySetter PropertyName="f_sgnetwork" Value="{m:Exp Str=infosearch.DataContext.f_sgnetwork}" Operator="f_sgnetwork = 'this'"/>          
                    	</a:Data.Prop>
						</TextBox>
					<Button Content="查  询" x:Name="searchbutton3" Click="{m:Exp Str=infosearch3.DataContext.Search()}" Margin="10,0,5,5" HorizontalAlignment="Left" Width="50"  Height="32" Padding="2" />			
				</StackPanel>
			</toolkit:DockPanel>
			<!--麟游民用气价查询条件-->
			<toolkit:DockPanel x:Name="infosearch4"  toolkit:DockPanel.Dock="Top" Margin="0" Background="#FFCFEFFF" d:LayoutOverrides="Width" Height="35" Visibility="Collapsed" >
				<a:Data.Context>
					<o:SearchObject />        	
				</a:Data.Context>
				<StackPanel Orientation="Horizontal">
					<TextBlock TextWrapping="Wrap" Text="发票号 从：" Margin="5,6,0,6" Height="25"/>
					<TextBox Text="{m:Bind Path=f_invoicenum1}" Margin="0,6"  Height="25" VerticalAlignment="Stretch" Width="80"  >
						<a:Data.Prop>
								<o:PropertySetter PropertyName="f_invoicenum1" Value="{m:Exp Str=infosearch.DataContext.f_invoicenum1}" Operator="f_invoicenum >= this"/>               
