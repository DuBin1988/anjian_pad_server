<UserControl
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    xmlns:a="clr-namespace:Com.Aote.Attachs;assembly=s2DLL"
    xmlns:o="clr-namespace:Com.Aote.ObjectTools;assembly=s2DLL"
    xmlns:b="clr-namespace:Com.Aote.Behaviors;assembly=s2DLL"
	xmlns:c="clr-namespace:Com.Aote.Controls;assembly=s2DLL"
    xmlns:m="clr-namespace:Com.Aote.Marks;assembly=s2DLL"
	xmlns:oo="clr-namespace:Com.Aote.ObjectTools"
    xmlns:i="http://schemas.microsoft.com/expression/2010/interactivity"
    xmlns:ei="http://schemas.microsoft.com/expression/2010/interactions"
    xmlns:data="clr-namespace:System.Windows.Data;assembly=System.Windows"
    xmlns:sdk="http://schemas.microsoft.com/winfx/2006/xaml/presentation/sdk"
    xmlns:toolkit="http://schemas.microsoft.com/winfx/2006/xaml/presentation/toolkit"
	xmlns:System="clr-namespace:System;assembly=mscorlib"
	mc:Ignorable="d" FontSize="13"
	x:Class="Com.Aote.Pages.机表冲正" d:DesignWidth="1024" d:DesignHeight="768">

	<Grid x:Name="LayoutRoot">
		<m:ResourceLoad Margin="0">
			<!--折子对象-->
			<oo:XYZZObj Name="zzobj"/>
			<!--机表用户对象-->
			<o:GeneralObject Name="kbusers" NotEmpty="True" EntityType="t_userfiles" WebClientInfo="{StaticResource dbclient}"
				Path="{m:Exp Str=zzobj.ReadData!\=null:$one/from t_userfiles where f_userid\=\'\{zzobj.ReadData\}\' and f_gasmeterstyle\=\'机表\'$\,userfilego.f_userid!\=null:$one/from t_userfiles where f_userid\=\'\{userfilego.f_userid\}\' and f_gasmeterstyle\=\'机表\'$\,null}"  
				IsInit="{m:Exp Str=zzprint.State\=\=$End$ |or| zzprint.State\=\=$Cancle$}">
				<!--折子号-->
				<o:PropertySetter PropertyName="f_zherownum" 
				NoDependValue="{m:Exp Str=SaveAction\=>Saving!\=null:sellinggas.f_zherownum.ToInt()\,0}"></o:PropertySetter>        		
				<o:PropertySetter PropertyName="f_cumulativepurchase" NoDependValue="{m:Exp Str=userfilego.f_upbuynum}" />
				<o:PropertySetter PropertyName="f_metergasnums" NoDependValue="{m:Exp Str=userfilego.f_premetergasnums}" />
				<o:PropertySetter PropertyName="f_beginfee" NoDependValue="{m:Exp Str=userfilego.f_beginfee!\=null:userfilego.f_beginfee\,null}" />
				<o:PropertySetter PropertyName="f_zhye" NoDependValue="{m:Exp Str=userfilego.f_zhye}" />
			</o:GeneralObject>
			
		   	<!--售气收费对象-->
        	<o:GeneralObject Name="sellinggas" NotEmpty="True" LoadOnPathChanged="False" EntityType="t_sellinggas" WebClientInfo="{StaticResource dbclient}"
			 	Path="{m:Exp Str=$one/from t_sellinggas where id\=
				 (select max(id) from t_sellinggas where f_userid\=\'\{userfilego.f_userid\}\' and f_payfeevalid\=\'有效\')$}"
             	IsOld="{m:Exp Str=userfilego.f_userid!\=null}"
				IsInit="{m:Exp Str=zzprint.State\=\=$End$ |or| zzprint.State\=\=$Cancle$}">  
        		<o:PropertySetter PropertyName="f_zherownum" NoDependValue="{m:Exp Str=kbusers !\= null and kbusers.f_zherownum !\= null and kbusers.f_zherownum.ToInt()^24:kbusers.f_zherownum+1\,1}"/>
				
				
			</o:GeneralObject>
		   	<!--冲正对象-->
			<o:GeneralObject Name="userfilego" NotEmpty="True" EntityType="t_chongzheng" WebClientInfo="{StaticResource dbclient}" 
				IsInit="{m:Exp Str=zzprint.State\=\=$End$ |or| zzprint.State\=\=$Cancle$ |or| nouser\=>OK!\=null |or| cancle\=>Click!\=null}">
				<o:PropertySetter PropertyName="chongzhengtype" Default="{m:Exp Str=$机表冲正$}" />
			</o:GeneralObject>
			
			<!--发票对象-->
			<o:GeneralObject Name="fapiao" EntityType="t_fapiaoinfos" WebClientInfo="{StaticResource dbclient}" 
			IsInit="{m:Exp Str=SaveAction.State\=\=$End$}" 
			Path="{m:Exp Str=sellinggas.f_invoicenum !\= null:$one/from t_fapiaoinfos where f_userid \=\'\{sellinggas.f_userid\}\' and f_invoicenum \=\'\{sellinggas.f_invoicenum\}\'$\,$null$}">
		    </o:GeneralObject>
			
			<!--更新用户对象-->
			<b:HQLAction Name="UpdateUser" WebClientInfo="{StaticResource dbclient}" 
				HQL="{m:Exp Str=$update t_userfiles 
				set f_cumulativepurchase\=\{userfilego.f_upbuynum\}\,
				f_metergasnums\=\{userfilego.f_premetergasnums\}\, 
				f_beginfee\=\{userfilego.f_beginfee!\=null:userfilego.f_beginfee\,$null$\}\, 
				f_zhye\=\{userfilego.f_zhye\} 
				where f_userid\=\'\{sellinggas.f_userid\}\'}"/> 
			<!--更新售气记录为无效-->
			<b:HQLAction Name="UpdateSellgas"   WebClientInfo="{StaticResource dbclient}" 
				HQL="{m:Exp Str=$update t_sellinggas set f_payfeevalid\=\'无效\'  where id \= \{sellinggas.id\}}"/> 
			<!--更新发票记录为作废-->
			<b:HQLAction Name="Updatefapiao"   WebClientInfo="{StaticResource dbclient}" 
				HQL="{m:Exp Str=$update t_fapiaoinfos set f_fapiaostatue\=\'作废\'  where  id \= \{fapiao.id\}}"/> 		    
			<!--更新抄表记录未交-->
			<b:HQLAction Name="UpdateHandplan"   WebClientInfo="{StaticResource dbclient}" 
				HQL="{m:Exp Str=$update t_handplan set shifoujiaofei\=\'否\'\ where id in (\{userfilego.f_useful\})}"/>  
			
			<!--保存对象 -->
			<b:BatchExcuteAction Name="SaveAction"  WebClientInfo="{StaticResource dbclient}">
				<!--更新售气记录无效-->
				<b:BatchInfo Source="{m:Exp Str=UpdateSellgas}" MethodName="InvokeToJson"/> 
		   		<!--保存冲正记录-->
				<b:BatchInfo Source="{m:Exp Str=userfilego}" MethodName="SaveToJson"/>
				<!--更新用户记录-->
				<b:BatchInfo Source="{m:Exp Str=kbusers}" MethodName="SaveToJson"/> 
		  		<!--更新抄表记录未交-->
				<b:BatchInfo Source="{m:Exp Str=UpdateHandplan}" MethodName="InvokeToJson"/> 
		  		<!--更新发票作废 CanSave="{m:Exp Str=userfilego.f_invoicenum !\=null}"-->
				<b:BatchInfo Source="{m:Exp Str=Updatefapiao}" MethodName="InvokeToJson" />
			</b:BatchExcuteAction>
			
			<!--用户列表部分-->
			<o:SearchObject Name="search"/>
			<o:PagedObjectList Name="userfileslist" WebClientInfo="{StaticResource dbclient}"
		       	Path="{m:Exp Str='$from t_userfiles where {search.Condition}'}"
		       	PageIndex="{m:Exp Str=pager.PageIndex}" PageSize="{m:Exp Str=pager.PageSize}" SumNames="id"/>
		</m:ResourceLoad>
		<Grid >
			<Grid.ColumnDefinitions>
				<ColumnDefinition Width="0.400*"/>
				<ColumnDefinition Width="0.600*"/>
			</Grid.ColumnDefinitions>
			<c:BusyIndicatorEx BusyContent="正在查询，请稍等..." IsBusy="{m:Exp Str=userfileslist.IsBusy}">
				<toolkit:DockPanel HorizontalAlignment="Stretch" Height="Auto" VerticalAlignment="Stretch" Margin="0">
					<toolkit:DockPanel  x:Name="userfilesearch" DataContext="{m:Exp Str=search}" toolkit:DockPanel.Dock="Top" Margin="0,0,0,0" Background="#FFCFEFFF" Width="Auto"  >
						<Grid>
							<Grid.RowDefinitions>
								<RowDefinition Height="28"/>
								<RowDefinition Height="28"/>
							</Grid.RowDefinitions>
							<Grid.ColumnDefinitions>
								<ColumnDefinition Width="100"/>
								<ColumnDefinition Width="50*"/>
								<ColumnDefinition Width="50*"/>
							</Grid.ColumnDefinitions>
							<TextBlock TextWrapping="Wrap" Text="用户编号：" Margin="5,4,0,4" Height="25" FontSize="13.333" HorizontalAlignment="Right" />
							<TextBox TextWrapping="Wrap" Grid.Column="1" Text="{m:Bind Path=f_userid}" Width="Auto" Margin="19,3,0,0" Height="25" FontSize="13.333" VerticalAlignment="Stretch" HorizontalAlignment="Stretch" >
								<a:Data.Prop>
									<o:PropertySetter PropertyName="f_userid"  Operator="f_userid like 'this%'"/>          
								</a:Data.Prop>
							</TextBox>
							<TextBlock TextWrapping="Wrap" Grid.Row="1" Text="用户姓名：" Margin="5,4,0,4" Height="25" FontSize="13.333" HorizontalAlignment="Right" />
							<TextBox TextWrapping="Wrap" Grid.Row="1" Grid.Column="1" Text="{m:Bind Path=f_username}" Width="Auto" Margin="19,2,0,1" Height="25" FontSize="13.333" HorizontalAlignment="Stretch" >
								<a:Data.Prop>
									<o:PropertySetter PropertyName="f_username"  Operator="f_username like 'this%'"/>          
								</a:Data.Prop>
							</TextBox>
							<Button Content="查  询" x:Name="searchbutton" Grid.Row="1" Grid.Column="2" Click="{m:Exp Str=search.Search()}"  FontSize="13.333" HorizontalAlignment="Left" Width="75" Margin="25,0,0,0"/>
						</Grid>
					</toolkit:DockPanel>
					<sdk:DataPager x:Name="pager" Source="{Binding ItemsSource.Count, Converter={StaticResource DataPagerConverter}, ElementName=userfiles}" PageSize="15" Margin="0" toolkit:DockPanel.Dock="Bottom"/>
					<sdk:DataGrid x:Name="userfiles" Margin="0"  FontSize="13.333" ItemsSource="{m:Exp Str=userfileslist}"
  						SelectedItem="{m:Exp Str=userfileslist[data.f_userid\=\=userfilego.f_userid].First()}"
						AutoGenerateColumns="False" Width="Auto" Background="{x:Null}" IsReadOnly="True" >
						<sdk:DataGrid.Columns>
							<sdk:DataGridTextColumn d:IsLocked="True" Header="序  号" Binding="{Binding Index}"/>
							<sdk:DataGridTextColumn Header="用户编号" Binding="{Binding f_userid}"/>
							<sdk:DataGridTextColumn d:IsLocked="True" Header="用户姓名" Binding="{Binding f_username}"/>
							<sdk:DataGridTextColumn Header="用户类型" Binding="{Binding f_usertype}"/>
							<sdk:DataGridTextColumn Header="气表底数" Binding="{Binding f_gasmeteraccomodations}"/>
							<sdk:DataGridTextColumn Header="卡号" Binding="{Binding f_cardid}"/>
							<sdk:DataGridTextColumn Header="表号" Binding="{Binding f_meternumber}"/>
							<sdk:DataGridTextColumn Header="气表品牌" Binding="{Binding f_gaswatchbrand}"/>
							<sdk:DataGridTextColumn Header="气表型号" Binding="{Binding f_metertype}"/>
							<sdk:DataGridTextColumn Header="额定流量" Binding="{Binding f_flownum}"/>
							<sdk:DataGridTextColumn Header="特殊限购" Binding="{Binding f_xiangou}"/>
							<sdk:DataGridTextColumn Header="地址" Binding="{Binding f_address}"/>
							<sdk:DataGridTextColumn Header="小区名称" Binding="{Binding f_districtname}"/>
							<sdk:DataGridTextColumn Header="门牌号" Binding="{Binding f_apartment}"/>
							<sdk:DataGridTextColumn Header="电  话" Binding="{Binding f_phone}"/>
							<sdk:DataGridTextColumn Header="用气性质" Binding="{Binding f_gasproperties}"/>
							<sdk:DataGridTextColumn Header="气价类型" Binding="{Binding f_gaspricetype}"/>
							<sdk:DataGridTextColumn Header="气价" Binding="{Binding f_gasprice}"/>
							<sdk:DataGridTextColumn Header="有壁挂炉" Binding="{Binding f_wallhangboiler}"/>
							<sdk:DataGridTextColumn Header="账户余额" Binding="{Binding f_zhye}"/>
							<sdk:DataGridTextColumn Header="总购气量" Binding="{Binding f_cumulativepurchase}"/>
							<sdk:DataGridTextColumn Header="当前表购气量" Binding="{Binding f_metergasnums}"/>
							<sdk:DataGridTextColumn Header="本次剩余补气量" Binding="{Binding f_thisnextaddgasamount}"/>
							<sdk:DataGridTextColumn Header="维护费日期" Binding="{Binding f_beginfee, StringFormat=yyyy-MM}"/>
							<sdk:DataGridTextColumn Header="发卡日期" Binding="{Binding f_givecarddate, StringFormat=yyyy-MM-dd}"/>
							<sdk:DataGridTextColumn Header="是否发卡" Binding="{Binding f_whethergivecard}"/>
							<sdk:DataGridTextColumn Header="用户状态" Binding="{Binding f_userstate}"/>
							<sdk:DataGridTextColumn Header="最后购气量" Binding="{Binding f_finallybought}"/>
							<sdk:DataGridTextColumn Header="最后购气日期" Binding="{Binding f_finabuygasdate, StringFormat=yyyy-MM-dd}"/>
							<sdk:DataGridTextColumn Header="表安装日期" Binding="{Binding f_watchinstalldate, StringFormat=yyyy-MM-dd}"/>
							<sdk:DataGridTextColumn Header="所属公司" Binding="{Binding f_filiale}"/>
							<sdk:DataGridTextColumn Header="网点" Binding="{Binding f_yytdepa}"/>
							<sdk:DataGridTextColumn Header="建档人" Binding="{Binding f_yytoper}"/>
							<sdk:DataGridTextColumn Header="建档日期" Binding="{Binding f_yytdate, StringFormat=yyyy-MM-dd}"/>
							<sdk:DataGridTextColumn Header="时间" Binding="{Binding f_yyttime, StringFormat=hh:mm:ss tt}"/>
						</sdk:DataGrid.Columns>
					</sdk:DataGrid>
				</toolkit:DockPanel></c:BusyIndicatorEx>
			<c:BusyIndicatorEx BusyContent="正在工作，请稍等" IsBusy="{m:Exp Str=sellinggas.IsBusy or SaveAction.IsBusy}" Margin="5,0,0,0" BorderThickness="0" BorderBrush="{x:Null}" Background="{x:Null}" Grid.Column="1" >
				<Grid  x:Name="userfile" DataContext="{m:Exp Str=userfilego}">
					
					<!--按钮-->
					<!--非民用主操作区-->
					<toolkit:DockPanel >
						<toolkit:DockPanel toolkit:DockPanel.Dock="Bottom"  Background="#FFC3E4FD">
							<StackPanel Orientation="Horizontal">
								<Button x:Name="save" Content="冲    正" Margin="5" 
									IsEnabled="{m:Exp Str=!userfile.DataContext.HasErrors}"
									Click="{m:Exp Str=SaveAction.Invoke()}" Width="50" />
								<Button Name="cancle" Content="撤  销" Margin="5" FontSize="12" Click="{m:Exp Str=sellinggas.New()}" Width="50"/>
							</StackPanel>
							<StackPanel Orientation="Horizontal" HorizontalAlignment="Right"/>
							 
						</toolkit:DockPanel>
						<Grid   Margin="0" x:Name="kbfee1"   >
							<Grid.Background>
								<LinearGradientBrush EndPoint="0.5,1" MappingMode="RelativeToBoundingBox" StartPoint="0.5,0">
									<GradientStop Color="#FFF2F8FF" Offset="0"/>
									<GradientStop Color="#FFDDF2FD" Offset="1"/>
								</LinearGradientBrush>
							</Grid.Background>
							<Grid.RowDefinitions>
								<RowDefinition Height="37"/>
								<RowDefinition Height="37"/>
								<RowDefinition Height="37"/>
								<RowDefinition Height="37"/>
								<RowDefinition Height="1"/>
								<RowDefinition Height="37"/>
								<RowDefinition Height="37"/>
								<RowDefinition Height="37"/>
								<RowDefinition Height="1"/>
								<RowDefinition Height="30"/>
								<RowDefinition Height="37"/>
								<RowDefinition Height="37"/>
								<RowDefinition Height="1"/>
								<RowDefinition Height="37"/>
								<RowDefinition Height="36"/>
								<RowDefinition Height="40"/>
							</Grid.RowDefinitions>
							<Grid.ColumnDefinitions>
								<ColumnDefinition Width="130"/>
								<ColumnDefinition Width="1*"/>
								<ColumnDefinition Width="130"/>
								<ColumnDefinition Width="1*"/>
								<ColumnDefinition Width="130"/>
								<ColumnDefinition Width="1*"/>
							</Grid.ColumnDefinitions>
							<TextBlock Text="用户编号：" Margin="0,6" TextWrapping="Wrap"  TextAlignment="Right" Height="25"/>
							<TextBlock Text="用户/单位名称：" Margin="0,6" TextWrapping="Wrap"  TextAlignment="Right" Grid.Column="2" Height="25"/>
							<TextBlock Text="地　　址：" Margin="0,6" Grid.Row="2" TextWrapping="Wrap" TextAlignment="Right" Height="25" Grid.Column="2" RenderTransformOrigin="1.638,0.44"/>
							<TextBlock Text="小区名称：" Margin="0,6" Grid.Row="2" TextWrapping="Wrap" TextAlignment="Right" Height="25"/>
							<TextBlock Text="气表品牌：" Margin="0,6" Grid.Row="3" TextWrapping="Wrap" TextAlignment="Right" Height="25"/>
							<TextBlock Text="气表型号：" Margin="0,6" Grid.Row="3" TextWrapping="Wrap" TextAlignment="Right" Grid.Column="2" Height="25"/>
							<TextBlock Margin="0,6" Grid.Row="5" TextWrapping="Wrap" Text="气价类型：" TextAlignment="Right" Grid.Column="2" Height="25"/>
							<TextBlock Text="气　　价：" Grid.Column="4"  TextWrapping="Wrap" TextAlignment="Right" Grid.Row="5" Margin="0,6" Height="25"/>
							<TextBlock Text="上次总累计购气量："  TextWrapping="Wrap" TextAlignment="Right" Grid.Row="6" Margin="0,6" Height="25"/>
							<TextBlock Text="用户类型：" Margin="1,6,-1,6" Grid.Row="1" TextWrapping="Wrap" TextAlignment="Right" Height="25"/>
							<TextBlock Text="用气性质：" Margin="0,6" Grid.Row="5" TextWrapping="Wrap" TextAlignment="Right" Height="25"/>
							<TextBlock Text="维管费起始月份：" Margin="0,6" Grid.Row="7" TextWrapping="Wrap" TextAlignment="Right" Height="25"/>
							<TextBlock Text="维管费截止月份：" Margin="0,6" Grid.Row="7" TextWrapping="Wrap" TextAlignment="Right" Height="25" Grid.Column="2"/>
							<TextBlock Text="用 气 量：" Margin="3,2,-3,3" TextWrapping="Wrap" TextAlignment="Right" Grid.Row="9" Width="130" Height="25" Grid.Column="4"/>
							<TextBlock Text="用气金额：" Margin="3,6,1,6" TextWrapping="Wrap" TextAlignment="Right" Grid.Row="10" Grid.Column="4" Height="25"/>
							<TextBlock Text="付款方式：" Margin="0,6" TextWrapping="Wrap" TextAlignment="Right" Grid.Row="11" Height="25"/>
							<TextBlock Text="收　　款：" Grid.Column="2" Margin="0,6" Grid.Row="11" TextWrapping="Wrap" TextAlignment="Right" Height="25"/>
							<TextBlock Text="应交金额：" Margin="0,6" Grid.Row="11" TextWrapping="Wrap" TextAlignment="Right" Height="25" Grid.Column="4"/>
							<TextBlock Text="本期结余：" Margin="0,5,0,7" Grid.Row="10" TextWrapping="Wrap" TextAlignment="Right" Height="25"/>
							<TextBox x:Name="f_userid" IsReadOnly="False" Text="{m:Bind Path=f_userid}"  Grid.Column="1" Margin="0,7,0,5" TextWrapping="Wrap" Grid.ColumnSpan="1" Height="25" VerticalAlignment="Stretch"  >
								<a:Data.Prop>
									<o:PropertySetter PropertyName="f_userid" NotNull="True"
									NoDependValue="{m:Exp Str=userfiles.SelectedItem!\=null:userfiles.SelectedItem.f_userid\,userfilego.f_userid}"/>
								</a:Data.Prop>
							</TextBox>
							<TextBox Text="{m:Bind Path=f_username}" IsReadOnly="True" Grid.Column="3" Margin="0,6" TextWrapping="Wrap" Grid.ColumnSpan="1" Height="25" VerticalAlignment="Stretch"  >
								<a:Data.Prop>
									<o:PropertySetter PropertyName="f_username" NoDependValue="{m:Exp Str=sellinggas.f_username}" NotNull="True"/>
								</a:Data.Prop>
							</TextBox>
							<CheckBox IsChecked="{m:Bind Path=f_dibaohu}"  Content="低保户" Margin="-18,7,0,5" Grid.Row="0" Grid.Column="5" Height="25" VerticalAlignment="Stretch" HorizontalAlignment="Left" IsEnabled="False" Width="62">
								<a:Data.Prop>
									<o:PropertySetter PropertyName="f_dibaohu" NoDependValue="{m:Exp Str=sellinggas.f_dibaohu}"/>
								</a:Data.Prop>
							</CheckBox>
							<TextBox Text="{m:Bind Path=f_idnumber}" IsReadOnly="True" Grid.Column="1" Margin="0,6" Grid.Row="1" TextWrapping="Wrap" Height="25" VerticalAlignment="Stretch" Visibility="Collapsed" >
								<a:Data.Prop>
									<o:PropertySetter PropertyName="f_idnumber" Value="{m:Exp Str=sellinggas.f_idnumber}"/>
								</a:Data.Prop>
							</TextBox>
							<TextBox Text="{m:Bind Path=f_address}" IsReadOnly="True" Grid.Column="3" Margin="0,6" Grid.Row="2" TextWrapping="Wrap" Grid.ColumnSpan="1" Height="25" VerticalAlignment="Stretch" >
								<a:Data.Prop>
									<o:PropertySetter PropertyName="f_address" NoDependValue="{m:Exp Str=sellinggas.f_address}"/>
								</a:Data.Prop>
							</TextBox>	
							
							<TextBox Text="{m:Bind Path=f_districtname}" IsReadOnly="True" Grid.Column="1" Margin="0,6" Grid.Row="2" TextWrapping="Wrap" Grid.ColumnSpan="1" Height="25" VerticalAlignment="Stretch" >
								<a:Data.Prop>
									<o:PropertySetter PropertyName="f_districtname" NoDependValue="{m:Exp Str=sellinggas.f_districtname}"/>
								</a:Data.Prop>
							</TextBox>
							<TextBox Text="{m:Bind Path=f_gaswatchbrand}" Grid.Column="1" Margin="0,6" Grid.Row="3" IsReadOnly="True" Height="25" VerticalAlignment="Stretch"   >
								<a:Data.Prop>
									<o:PropertySetter PropertyName="f_gaswatchbrand" NoDependValue="{m:Exp Str=sellinggas.f_gaswatchbrand}"/>
								</a:Data.Prop>
							</TextBox>
							<TextBox Text="{m:Bind Path=f_metertype}" Grid.Column="3" Margin="0,6" Grid.Row="3" IsReadOnly="True" Height="25" VerticalAlignment="Stretch"  >
								<a:Data.Prop>
									<o:PropertySetter PropertyName="f_metertype" NoDependValue="{m:Exp Str=sellinggas.f_metertype}"/>
								</a:Data.Prop>
							</TextBox>
							<TextBox Text="{m:Bind Path=f_gaspricetype}"  Grid.Column="3" Margin="0,6" Grid.Row="5" IsReadOnly="True" Height="25" VerticalAlignment="Stretch"  >
								<a:Data.Prop>
									<o:PropertySetter PropertyName="f_gaspricetype" NoDependValue="{m:Exp Str=sellinggas.f_gaspricetype}"/>
								</a:Data.Prop>	
							</TextBox>
							<TextBox Text="{m:Bind Path=f_gasprice}" Grid.Column="5"  Grid.Row="5" TextWrapping="Wrap" IsReadOnly="True" HorizontalAlignment="Stretch" Width="Auto" Margin="0,6" Grid.ColumnSpan="1" Height="25" VerticalAlignment="Stretch" >
								<a:Data.Prop>
									<o:PropertySetter PropertyName="f_gasprice" NoDependValue="{m:Exp Str=sellinggas.f_gasprice}"/>
								</a:Data.Prop>
							</TextBox>
							<TextBox Text="{m:Bind Path=f_unitfee}" Grid.Column="5"  Grid.Row="0" TextWrapping="Wrap" IsReadOnly="True" HorizontalAlignment="Right" Width="8" Margin="0,10,98,2" Height="25" VerticalAlignment="Stretch" Grid.RowSpan="1" Visibility="Collapsed" >
								<a:Data.Prop>
									<o:PropertySetter PropertyName="f_unitfee"     Value="{m:Exp Str=sellinggas.f_unitfee}"/>
								</a:Data.Prop>
							</TextBox>
							<TextBox Text="{m:Bind Path=f_upbuynum}" Grid.Column="1" Margin="0,6" Grid.Row="6" TextWrapping="Wrap" IsReadOnly="True" HorizontalAlignment="Stretch" Height="25" VerticalAlignment="Stretch" >
								<a:Data.Prop>
									<o:PropertySetter PropertyName="f_upbuynum" NoDependValue="{m:Exp Str=sellinggas.f_upbuynum}"/>
								</a:Data.Prop>
							</TextBox>
							 
							<TextBox Text="{m:Bind Path=f_usertype}" Grid.Column="1" Margin="1,6,-1,6" Grid.Row="1"  HorizontalAlignment="Stretch" IsReadOnly="True" Height="25" VerticalAlignment="Stretch" >
								<a:Data.Prop>
									<o:PropertySetter PropertyName="f_usertype" NoDependValue="{m:Exp Str=sellinggas.f_usertype}"/>
								</a:Data.Prop>	
