			<o:GeneralObject Name="userfiles" EntityType="t_otherfee" WebClientInfo="{StaticResource dbclient}"
             Path="{m:Exp Str=pipelines.SelectedItem.id !\= null:$one/from t_otherfee where id\=\'\{pipelines.SelectedItem.id\}\'$\,$abc$}" 
