			</a:Data.Prop>
		</TextBox>
		<TextBox Grid.Row="1" Grid.Column="1" Text="{m:Bind Path=f_userid}" >
			<a:Data.Prop>
					<o:PropertySetter PropertyName="f_userid" NotNull="True"/>
			</a:Data.Prop>
