<UserControl
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    xmlns:a="clr-namespace:Com.Aote.Attachs;assembly=s2DLL"
    xmlns:o="clr-namespace:Com.Aote.ObjectTools;assembly=s2DLL"
	xmlns:oo="clr-namespace:Com.Aote.ObjectTools"
    xmlns:b="clr-namespace:Com.Aote.Behaviors;assembly=s2DLL"
	xmlns:c="clr-namespace:Com.Aote.Controls;assembly=s2DLL"
    xmlns:m="clr-namespace:Com.Aote.Marks;assembly=s2DLL"
    xmlns:i="http://schemas.microsoft.com/expression/2010/interactivity"
    xmlns:ei="http://schemas.microsoft.com/expression/2010/interactions"
    xmlns:data="clr-namespace:System.Windows.Data;assembly=System.Windows"
    xmlns:sdk="http://schemas.microsoft.com/winfx/2006/xaml/presentation/sdk"
    xmlns:toolkit="http://schemas.microsoft.com/winfx/2006/xaml/presentation/toolkit"
	xmlns:System="clr-namespace:System;assembly=mscorlib"
	mc:Ignorable="d" FontSize="13.333"
	x:Class="Com.Aote.Pages.转费收费" FontFamily="Microsoft YaHei" d:DesignWidth="800" d:DesignHeight="600">

	<Grid x:Name="LayoutRoot">
		<m:ResourceLoad Margin="0,0,-101,0">
			<!--计算维护费结果-->
			<o:GeneralObject  Name="whf" WebClientInfo="{StaticResource serveraddress}" 
				EntityType="t_sellinggas"  Path="{m:Exp Str=kbfee.f_beginfee!\=null and kbfee.f_endfee!\=null:$ca/whf/\{kbfee.f_beginfee.ToString($yyyy-MM-dd$)\}/\{kbfee.f_endfee.ToString($yyyy-MM-dd$)\}$\,$abc$}">
			</o:GeneralObject>
			
			<!--限购月数-->
			<o:GeneralObject Name="xgys" EntityType="t_singlevalue" WebClientInfo="{StaticResource dbclient}" 
		  		Path="one/from t_singlevalue where name='限购月数' "   ></o:GeneralObject>
			<!--非民用限购参数对象-->
        	<o:GeneralObject Name="gsxiangou" EntityType="t_gsxiangou" WebClientInfo="{StaticResource dbclient}"
                Path="{m:Exp Str=$one/from t_gsxiangou where f_fanwei &lt;\= \{kbusers.f_flownum\} and f_daofanwei &gt; \{kbusers.f_flownum\}}">
			</o:GeneralObject>
			 
			<!--历史交费列表-->
			<o:ObjectList Name="kbsellgas" WebClientInfo="{StaticResource dbclient}"
				Path="{m:Exp Str=$from t_handplan where f_userid\=\'\{kbfee1.DataContext.f_userid\}\' and shifoujiaofei\=\'否\' order by id$}"
				IsOld="{m:Exp Str=SaveAction.State\=\=$End$}" />
			
			<!--是否列表-->
			<o:ObjectList Name="shifoulist" WebClientInfo="{StaticResource dbclient}" Path="from paramvalue where param.name='是与否' order by id"/>
			<!--付款方式列表-->
			<o:ObjectList Name="paylist" WebClientInfo="{StaticResource dbclient}" Path="from paramvalue where param.name='转气付费方' order by id"/>
			<!--写卡类型列表-->
			<o:ObjectList Name="xielist" WebClientInfo="{StaticResource dbclient}" Path="from paramvalue where param.name='写卡类型' order by id"/>
			
			<!--混合气价对象 -->
        	<o:GeneralObject Name="hunhes" EntityType="t_hunhe" WebClientInfo="{StaticResource dbclient}"
             	Path="{m:Exp Str=$one/from t_hunhe }">
			</o:GeneralObject>	
			
			<!--单值对象-->
			<o:GeneralObject Name="fbffobj" EntityType="t_singlevalue" WebClientInfo="{StaticResource dbclient}" 
		  		Path="one/from t_singlevalue where name='民用低保户月维护费' "   ></o:GeneralObject>
			<o:GeneralObject Name="mwhfobj" EntityType="t_singlevalue" WebClientInfo="{StaticResource dbclient}" 
		  		Path="one/from t_singlevalue where name='民用月维护费' "   ></o:GeneralObject>
			<o:GeneralObject Name="fwhfobj" EntityType="t_singlevalue" WebClientInfo="{StaticResource dbclient}" 
		  		Path="one/from t_singlevalue where name='非民用月维护费' "   ></o:GeneralObject>
        	<o:GeneralObject Name="mxggasobj" EntityType="t_singlevalue" WebClientInfo="{StaticResource dbclient}" 
		  		Path="one/from t_singlevalue where name='民用月限购气量'"   ></o:GeneralObject>
			<o:GeneralObject Name="mzdgasobj" EntityType="t_singlevalue" WebClientInfo="{StaticResource dbclient}" 
		  		Path="one/from t_singlevalue where name='民用单次最大购气量'"   ></o:GeneralObject>
			<o:GeneralObject Name="fzdgasobj" EntityType="t_singlevalue" WebClientInfo="{StaticResource dbclient}" 
		  		Path="one/from t_singlevalue where name='非民用单次最大购气量'"   ></o:GeneralObject>	
			<o:GeneralObject Name="mygqlobj" EntityType="t_singlevalue" WebClientInfo="{StaticResource dbclient}" 
		  		Path="one/from t_singlevalue where name='民用购气量' "   ></o:GeneralObject>
			<o:GeneralObject Name="fapiaogeshi" EntityType="t_singlevalue" WebClientInfo="{StaticResource dbclient}" 
		  	Path="one/from t_singlevalue where name='发票格式'"   ></o:GeneralObject>	
			
			<!--用户对象-->
			<o:GeneralObject Name="kbusers" EntityType="t_userfiles" WebClientInfo="{StaticResource dbclient}"
             Path="{m:Exp Str=zzobj.ReadData!\=null:$one/from t_userfiles where f_userid\=\'\{zzobj.ReadData\}\'$\,kbfee1.DataContext.f_userid!\=null:$one/from t_userfiles where f_userid\=\'\{kbfee1.DataContext.f_userid\}\'$\,card.CardId\!\=$$ and card.Factory\!\=$$:$one/from t_userfiles where f_cardid\=\'\{card.CardId\}\' and f_aliasname\=\'\{card.Factory\}\'$\,$null$}"  
			 IsInit="{m:Exp Str=SaveAction\=\=$Error$ |or| RollbackAction\=\=$End$ |or| zzprint.State\=\=$End$}">
				<!--折子号-->
				<o:PropertySetter PropertyName="f_zherownum" 
				NoDependValue="{m:Exp Str=SaveAction\=>Saving!\=null:kbfee.f_zherownum.ToInt()\,0}"></o:PropertySetter>        		
				
			</o:GeneralObject>
			
			
			<!--用户对象-->
			<o:GeneralObject Name="kbusers2" EntityType="t_userfiles" WebClientInfo="{StaticResource dbclient}"
             Path="{m:Exp Str=$one/from t_userfiles where f_userid\=\'\{kbfee.f_inid\}\'$}"  
			 IsInit="{m:Exp Str=SaveAction\=\=$Error$ |or| RollbackAction\=\=$End$ |or| zzprint.State\=\=$End$}">
			
			</o:GeneralObject>
			
			<!--收费对象-->
        	<o:GeneralObject Name="kbfee" EntityType="t_switchmoney" WebClientInfo="{StaticResource dbclient}"
            IsInit="{m:Exp Str=SaveAction\=\=$Error$ |or| RollbackAction\=\=$End$ |or| zzprint.State\=\=$End$}">
	  			
			</o:GeneralObject>
			
			<!--发票对象-->
        	<o:GeneralObject Name="fpinfosobj" EntityType="t_fapiaoinfos" WebClientInfo="{StaticResource dbclient}" 
			IsInit="{m:Exp Str=SaveAction\=\=$Error$ |or| card.State\=\=$Error$ |or| zzprint.State\=\=$End$}">
        		<o:PropertySetter PropertyName="f_username" Value="{m:Exp Str=kbfee1.DataContext.f_username}"/>
				<o:PropertySetter PropertyName="f_invoicenum" Value="{m:Exp Str=kbfee1.DataContext.f_invoicenum}"/>
				<o:PropertySetter PropertyName="f_userid" Value="{m:Exp Str=kbfee1.DataContext.f_userid}" />
				<o:PropertySetter PropertyName="f_cardid" Value="{m:Exp Str=kbfee1.DataContext.f_cardid}"/>
				<o:PropertySetter PropertyName="f_gas" Value="{m:Exp Str=kbfee1.DataContext.f_pregas}" />
				<o:PropertySetter PropertyName="f_money" Value="{m:Exp Str=kbfee1.DataContext.f_preamount}" />
				<o:PropertySetter PropertyName="f_repairfee" Value="{m:Exp Str=kbfee1.DataContext.f_amountmaintenance}" />
				<o:PropertySetter PropertyName="f_totalfee" Value="{m:Exp Str=kbfee1.DataContext.f_totalcost}" />
				<o:PropertySetter PropertyName="f_sgnetwork" Value="{m:Exp Str=kbfee1.DataContext.f_sgnetwork}" />
				<o:PropertySetter PropertyName="f_sgoperator" Value="{m:Exp Str=kbfee1.DataContext.f_sgoperator}" />
				<o:PropertySetter PropertyName="f_fapiaotime" Value="{m:Exp Str=kbfee1.DataContext.f_deliverytime}" />
				<o:PropertySetter PropertyName="f_fapiaodate" Value="{m:Exp Str=kbfee1.DataContext.f_deliverydate}" />
				<o:PropertySetter PropertyName="f_filiale" Value="{m:Exp Str=kbfee1.DataContext.f_filiale}" />
				<o:PropertySetter PropertyName="f_fapiaotype" Default="购气"  />
				<o:PropertySetter PropertyName="f_fapiaostatue" Default="已用"  />
			</o:GeneralObject>
			
			
		
		    <!--保存-->
			<b:BatchExcuteAction Name="SaveAction" WebClientInfo="{StaticResource dbclient}">
				<!--用户对象-->
				<b:BatchInfo Source="{m:Exp Str=kbusers}" MethodName="SaveToJson"/>
				<!--更新维护费开始日期-->
			   	<b:BatchInfo Source="{m:Exp Str=UpdateBeginFeeAction}" MethodName="InvokeToJson"/>
				<!--收费对象，可能包含补气对象-->
				<b:BatchInfo Source="{m:Exp Str=kbfee1.DataContext}" MethodName="SaveToJson"/>
				<!--发票对象-->
				<b:BatchInfo Source="{m:Exp Str=fpinfosobj}" MethodName="SaveToJson"/>
			</b:BatchExcuteAction>
		
			<!--更新售气记录为无效-->
			<b:HQLAction Name="UpdateSellgas" WebClientInfo="{StaticResource dbclient}" 
			HQL="{m:Exp Str=$update t_sellinggas set f_payfeevalid\=\'无效\' where id \= \{kbfee.id\}}"/> 
			<!--更新发票记录为作废-->
			<b:HQLAction Name="Updatefapiao" WebClientInfo="{StaticResource dbclient}" 
			HQL="{m:Exp Str=$update t_fapiaoinfos set f_fapiaostatue\= \'未用\' where id \= \{fpinfosobj.id\}}"/> 
			<!--更新用户档案中相关信息-->
			<b:HQLAction Name="UpdateUserfile" WebClientInfo="{StaticResource dbclient}" 
			HQL="{m:Exp Str=$update t_userfiles set 
			f_cumulativepurchase\=\{kbfee.f_upbuynum\}\, 
			f_metergasnums\=\{kbfee.f_premetergasnums\}\,
			f_surplusgas\=\{kbfee.f_surplusgas\}\,
			f_beginfee\=\'\{kbfee.f_beginfee\}\'\,
			f_finallybought\=\{kbfee.f_finallybought\}\,
			f_finabuygasdate\=\'\{kbfee.f_finabuygasdate\}\'\,
			f_initcardsellgas\=\'\{kbfee.f_initcardsellgas\}\'\,
			f_times\=f_times - 1
			where id \= \{kbusers.id\}}"/> 
			<!--制卡失败后的自动撤销过程-->
			<b:BatchExcuteAction Name="RollbackAction" CanSave="{m:Exp Str=card.State\=\=$Error$}" WebClientInfo="{StaticResource dbclient}">
				<b:BatchInfo Source="{m:Exp Str=UpdateUserfile}" MethodName="InvokeToJson"/>
			    <b:BatchInfo Source="{m:Exp Str=UpdateSellgas}" MethodName="InvokeToJson"/> 
		   		<b:BatchInfo Source="{m:Exp Str=Updatefapiao}" MethodName="InvokeToJson"/> 
				
			</b:BatchExcuteAction>
			<b:HQLAction Name="UpdateBeginFeeAction"   WebClientInfo="{StaticResource dbclient}" 
			HQL="{m:Exp Str=$update t_userfiles set f_beginfee\= \'\{kbfee.f_beginfeehx\}\' where f_userid\=\'\{kbusers.f_userid\}\'}"/> 
		
		<!--折子对象-->
		<oo:XYZZObj Name="zzobj"/>
		
		<!--卡对象-->	
		<oo:NewGeneralICCard Name="card"   
		 CanSellGas="{m:Exp Str='SaveAction.State=>Completed==$End$ and kbfee.f_initcardsellgas!=$初始化卡购气标记$ and kbfee.f_initcardsellgas!=$补卡$' }"
		 CanRewriteCard="{m:Exp Str='SaveAction.State=>Completed==$End$ and (kbfee.f_initcardsellgas==$初始化卡购气标记$ or kbfee.f_initcardsellgas==$补卡$)'}"
		 CardId="{m:Exp Str=kbfee1.DataContext.f_cardid}"
		 Gas="{m:Exp Str=(kbfee.f_pregas+kbfee.f_surplusgas).ToInt()}"
		 Klx="{m:Exp Str='kbusers != null\:kbusers.klx\,null'}"		
		 Kzt="{m:Exp Str='kbusers=>DataLoaded!=null:(kbfee.f_initcardsellgas==$初始化卡购气标记$:0,1),0'}"
		 Dqdm="{m:Exp Str='kbusers != null \:kbusers.dqdm\,null'}"
		 Yhh="{m:Exp Str='kbusers != null \:kbusers.yhh\,null'}"
		 Tm="0"
		 Ljgql="{m:Exp Str='kbusers != null \:kbusers.f_cumulativepurchase\,null'}"
		 Bkcs="0"
		 Bjql="{m:Exp Str='kbusers != null \:kbusers.bjql\,null'}"
		 Czsx="{m:Exp Str='kbusers != null \:kbusers.czsx\,null'}"
		 Tzed="{m:Exp Str='kbusers != null \:kbusers.tzed\,null'}"	
		 Kmm="{m:Exp Str='kbusers != null \:kbusers.kmm\,null'}"
		 Sqrq="{m:Exp Str='SysTime.Now.ToString($yyyyMMdd$)'}"
	 	 NewPrice="{m:Exp Str='kbusers != null\:kbusers.f_gasprice*100\,0'}"
		 Sxrq="{m:Exp Str='SysTime.Now.ToString($yyyyMMdd$)'}"
		 Sxbj="{m:Exp Str=card.Factory \=\= $ZhenLan$ \:null\,$1$}"
		>
		</oo:NewGeneralICCard>
		</m:ResourceLoad>
		<Grid >
			<Grid.ColumnDefinitions>
				<ColumnDefinition Width="0.3*"/>
				<ColumnDefinition Width="0.7*"/>
			</Grid.ColumnDefinitions >
			<c:BusyIndicatorEx BusyContent="正在进行卡操作，请稍等" IsBusy="{m:Exp Str=card.IsBusy}" Width="Auto" HorizontalAlignment="Stretch" >
				<toolkit:DockPanel x:Name="userfilesearch"  Background="#FFCFEFFF" Margin="0" Width="Auto" HorizontalAlignment="Stretch" d:LayoutOverrides="Height" >
					<Grid Width="Auto">
						<Grid.ColumnDefinitions>
							<ColumnDefinition Width="100"/>
							<ColumnDefinition/>
						</Grid.ColumnDefinitions>
						<Grid.RowDefinitions>
							<RowDefinition Height="28"/>
							<RowDefinition Height="28"/>
							<RowDefinition Height="28"/>
							<RowDefinition Height="28"/>
							<RowDefinition Height="28"/>
							<RowDefinition Height="28"/>
							<RowDefinition Height="28"/>
						</Grid.RowDefinitions>
						<TextBlock TextWrapping="Wrap" Text="卡　　号：" Margin="0,6" HorizontalAlignment="Right"/>
						<TextBox Text="{m:Exp Str=card.CardId.NullToEmpty()\=>ReadCompleted}" IsReadOnly="True" TextWrapping="Wrap" Grid.Column="1"  a:ControlAttach.DefaultButton="searchbutton" Width="Auto" Margin="0,2,0,1" Height="25" d:LayoutOverrides="GridBox" />
						<TextBlock TextWrapping="Wrap" Grid.Row="1" Text="厂　　家：" Margin="0,6" HorizontalAlignment="Right"/>
						<TextBox TextWrapping="Wrap" Grid.Row="1" Grid.Column="1" IsReadOnly="True" Text="{m:Exp Str=card.Factory.NullToEmpty()\=>ReadCompleted}" a:ControlAttach.DefaultButton="searchbutton" Width="Auto" Margin="0,2,0,1" Height="25" d:LayoutOverrides="GridBox" />					
						<TextBlock TextWrapping="Wrap" Grid.Row="2" Text="卡上气量：" Margin="0,6" HorizontalAlignment="Right"/>
						<TextBlock TextWrapping="Wrap" Grid.Row="3" Text="购气次数：" Margin="0,6" HorizontalAlignment="Right"/>
				
						<TextBox TextWrapping="Wrap" IsReadOnly="True" Grid.Row="3" Grid.Column="1" Text="{m:Exp Str=card.BuyTimes\=>ReadCompleted}" a:ControlAttach.DefaultButton="searchbutton" Width="Auto" Margin="0,3,0,0" Height="25" d:LayoutOverrides="GridBox" />
				
						<TextBox TextWrapping="Wrap" IsReadOnly="True" Grid.Row="2" Grid.Column="1" Text="{m:Exp Str=card.Gas\=>ReadCompleted}" a:ControlAttach.DefaultButton="searchbutton" Width="Auto" Margin="0,3,0,0" Height="25" d:LayoutOverrides="GridBox" />

					</Grid>
				</toolkit:DockPanel>
			</c:BusyIndicatorEx>
			<c:BusyIndicatorEx 
				BusyContent="{m:Exp Str=$正在访问网络，请稍等...$}" 
				IsBusy="{m:Exp Str=kbusers.IsBusy or kbfee1.DataContext.IsBusy or SaveAction.IsBusy}"  Grid.Column="1" Margin="0" Grid.ColumnSpan="1" d:LayoutOverrides="GridBox">
				<toolkit:DockPanel>
					<StackPanel toolkit:DockPanel.Dock="Top">
						<Grid   Margin="0" x:Name="kbfee1" DataContext="{m:Exp Str=kbfee}" >
							<Grid.Background>
								<LinearGradientBrush EndPoint="0.5,1" MappingMode="RelativeToBoundingBox" StartPoint="0.5,0">
									<GradientStop Color="#FFF2F8FF" Offset="0"/>
									<GradientStop Color="#FFDDF2FD" Offset="1"/>
								</LinearGradientBrush>
							</Grid.Background>
							<Grid.RowDefinitions>
								<RowDefinition Height="37"/>
								<RowDefinition Height="37"/>
								<RowDefinition Height="37"/>
								<RowDefinition Height="37"/>
								<RowDefinition Height="1"/>
								<RowDefinition Height="37"/>
								<RowDefinition Height="37"/>
								<RowDefinition Height="37"/>
								<RowDefinition Height="37"/>
								<RowDefinition Height="1"/>
								<RowDefinition Height="37"/>
								<RowDefinition Height="37"/>
								<RowDefinition Height="37"/>
								<RowDefinition Height="37"/>
								<RowDefinition Height="37"/>
								<RowDefinition Height="37"/>
							</Grid.RowDefinitions>
							<Grid.ColumnDefinitions>
								<ColumnDefinition Width="130"/>
								<ColumnDefinition Width="0.4*"/>
								<ColumnDefinition Width="166"/>
								<ColumnDefinition Width="0.5*"/>
							</Grid.ColumnDefinitions>
							<TextBlock Text="转出编号：" Margin="0" TextWrapping="Wrap"  TextAlignment="Right" Height="25"/>
							<TextBlock Text="转出用户/单位名称：" Margin="0" TextWrapping="Wrap"  TextAlignment="Right" Grid.Column="2" Height="25"/>
							<TextBlock Text="转出原因：" Margin="0" Grid.Row="2" TextWrapping="Wrap" TextAlignment="Right" Height="25" VerticalAlignment="Center"/>
							<TextBlock Text="转气手续费：" Margin="0" Grid.Row="3" TextWrapping="Wrap" TextAlignment="Right" Height="25"/>
							<TextBlock Text="转费日期：" Margin="0" Grid.Row="1" TextWrapping="Wrap" TextAlignment="Right" Grid.Column="2" Height="25"/>
							<TextBlock Text="气    价："  TextWrapping="Wrap" TextAlignment="Right" Grid.Row="3" Margin="0" Height="25" Grid.Column="2"/>
							<TextBlock Text="经办人：" Margin="0,6" TextWrapping="Wrap" TextAlignment="Right" Grid.Row="2" Grid.Column="2" Height="25" HorizontalAlignment="Right" Width="130"/>
							<TextBlock Text="备    注：" Margin="0" Grid.Row="11" TextWrapping="Wrap" TextAlignment="Right" Height="25" VerticalAlignment="Center"/>
							<TextBlock Text="余 气 量：" Margin="0" Grid.Row="8" TextWrapping="Wrap" TextAlignment="Right" Height="25"/>
							<TextBlock Text="底    数：" Margin="0" Grid.Row="8" TextWrapping="Wrap" TextAlignment="Right" Height="25" Grid.Column="2"/>
							<TextBlock Text="转出气量：" Margin="0" TextWrapping="Wrap" TextAlignment="Right" Grid.Row="10" Width="130" Height="25"/>
							<TextBlock Text="转出气费金额：" Margin="0" TextWrapping="Wrap" TextAlignment="Right" Grid.Row="10" Grid.Column="2" Height="25"/>
							<TextBlock Text="用气押金转出：" Margin="0" TextWrapping="Wrap" TextAlignment="Right" Grid.Row="12" Height="25" Grid.Column="2"/>
							<TextBlock Text="付手续费方：" Grid.Column="2" Margin="0" Grid.Row="11" TextWrapping="Wrap" TextAlignment="Right" Height="25"/>
							<TextBox x:Name="f_userid" Text="{m:Bind Path=f_userid}"  Grid.Column="1" Margin="0,7,10.833,5" TextWrapping="Wrap" Grid.ColumnSpan="1" Height="25" VerticalAlignment="Stretch"  >
								<a:Data.Prop>
									<o:PropertySetter PropertyName="f_userid" />
								</a:Data.Prop>
							</TextBox>
							<Button Click="{m:Exp Str=kbusers.New();kbfee.New();kbsellgas.New();kbbysellgas.New();card.ReadCard()}" Content="读卡" Margin="-6.833,2,0,2" FontSize="12" Grid.Column="2" HorizontalAlignment="Left"/>
							<TextBox Text="{m:Bind Path=f_username}" IsReadOnly="True" Grid.Column="3" Margin="0" TextWrapping="Wrap" Grid.ColumnSpan="1" Height="25" VerticalAlignment="Stretch"  >
								<a:Data.Prop>
									<o:PropertySetter PropertyName="f_username" Value="{m:Exp Str=kbusers.f_username}"/>
								</a:Data.Prop>
							</TextBox>
							<TextBox Text="{m:Bind Path=f_idnumber}" IsReadOnly="True" Grid.Column="1" Margin="0" Grid.Row="1" TextWrapping="Wrap" Height="25" VerticalAlignment="Stretch" Visibility="Collapsed" >
								<a:Data.Prop>
									<o:PropertySetter PropertyName="f_idnumber" Value="{m:Exp Str=kbusers.f_idnumber}"/>
								</a:Data.Prop>
							</TextBox>	
							
							<TextBox Text="{m:Bind Path=f_outfeereason}" IsReadOnly="False" Grid.Column="1" Margin="0,0,0.222,0" Grid.Row="2" TextWrapping="Wrap" Grid.ColumnSpan="1" Height="Auto" VerticalAlignment="Center" >
								<a:Data.Prop>
									<o:PropertySetter PropertyName="f_outfeereason"  />
								</a:Data.Prop>
							</TextBox>
							<TextBox Text="{m:Bind Path=f_gaswatchbrand}" Grid.Column="1" Margin="0" Grid.Row="3" IsReadOnly="False" Height="25" VerticalAlignment="Stretch"   >
								<a:Data.Prop>
									<o:PropertySetter PropertyName="f_gaswatchbrand"  />
								</a:Data.Prop>
							</TextBox>
							<TextBox Text="{m:Bind Path=f_unitfee}" Grid.Column="5"  Grid.Row="0" TextWrapping="Wrap" IsReadOnly="True" HorizontalAlignment="Right" Width="8" Margin="0,6,98,-1" Height="25" VerticalAlignment="Stretch" Grid.RowSpan="1" d:LayoutOverrides="HorizontalAlignment" Visibility="Collapsed" >
								<a:Data.Prop>
									<o:PropertySetter PropertyName="f_unitfee" Value="{m:Exp Str=(kbfee.f_usertype\=\=$民用$ and kbfee.f_dibaohu!\=$True$):mwhfobj.value\,(kbfee.f_usertype\=\=$民用$ and kbfee.f_dibaohu\=\=$True$):fbffobj.value\,0}"/>
								</a:Data.Prop>
							</TextBox>
							<TextBox Text="{m:Bind Path=f_allgasnum}" Grid.Column="1" Margin="0" Grid.Row="7" TextWrapping="Wrap" IsReadOnly="True" HorizontalAlignment="Stretch" Height="25" VerticalAlignment="Stretch" >
								<a:Data.Prop>
									<o:PropertySetter PropertyName="f_allgasnum"  Value="{m:Exp Str=kbusers.f_metergasnums}"/>
								</a:Data.Prop>
							</TextBox>
							
							
							<TextBox Text="{m:Bind Path=f_remark}" Grid.Column="1" Margin="0,1,-0.111,0" Grid.Row="11" IsReadOnly="False" Height="Auto" VerticalAlignment="Center" Grid.ColumnSpan="1" >
								<a:Data.Prop>
									<o:PropertySetter PropertyName="f_remark"  />
								</a:Data.Prop>
							</TextBox>
							<!--从卡上读购气次数{m:Exp Str=card.BuyTimes\=>ReadCompleted+1}-->
							<TextBox Text="{m:Bind Path=f_outgasno}" FontWeight="Bold" Foreground="Red" Grid.Column="1" Margin="0" TextWrapping="Wrap" Grid.ColumnSpan="1" Grid.Row="10" Height="25" VerticalAlignment="Stretch"  />
							<TextBox Text="{m:Bind Path=f_outmoney}"  Grid.Column="3" Margin="0" TextWrapping="Wrap" Grid.ColumnSpan="1" IsReadOnly="True" Grid.Row="10" Height="25" VerticalAlignment="Stretch"  >
								<a:Data.Prop>
									<o:PropertySetter PropertyName="f_outmoney" />
								</a:Data.Prop>
							</TextBox>
